ersion":3,"sources":["product-select.vue"],"names":[],"mappings":";;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;AA6DA;;AAEA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA","file":"product-select.vue","sourceRoot":"src/views/pages/brand/setting/mina/components#/h5/form-components#","sourcesContent":["<template>\n  <div :class=\"module('content')\">\n    <div :class=\"module('content-label')\">{{ title }}</div>\n    <div :class=\"module('content-desc')\">\n      {{ desc }}\n    </div>\n    <a-alert\n      class=\"mg-t16\"\n      v-if=\"value.list.length > maxNum - 1\"\n      message=\"当前数量已达上限，可在删除后重新添加\"\n      banner\n    ></a-alert>\n    <st-form class=\"mg-t16\">\n      <div\n        :class=\"module('box')\"\n        class=\"h5-form-component__add\"\n        @click=\"openModal\"\n        v-if=\"value.list.length < maxNum\"\n      >\n        <st-icon type=\"add\" size=\"14px\" color=\"rgba(62, 77, 92, 1)\" />\n        <div :class=\"module('add-label')\">{{ placeholder }}</div>\n      </div>\n      <st-draggable\n        v-model=\"value.list\"\n        :animation=\"200\"\n        :class=\"module('draggable-wrap')\"\n        @change=\"onChange\"\n      >\n        <div\n          v-for=\"(item, index) in value.list\"\n          :key=\"index\"\n          :class=\"module('box')\"\n          style=\"cursor: move;\"\n        >\n          <div :class=\"module('del')\" @click=\"delectItem(index)\">\n            <st-icon type=\"delete\" color=\"#FF5E41\" size=\"16px\" />\n          </div>\n          <img\n            :class=\"module('bg')\"\n            style=\"object-fit: cover;\"\n            :src=\"imgFilter(item.image_url, { w: 180, h: 100 })\"\n            width=\"180\"\n            height=\"100\"\n          />\n          <div :class=\"module('name')\" class=\"text-overflow\">\n            {{ getViewName(item) }}\n          </div>\n          <a-tooltip :class=\"module('draggable-icon')\" placement=\"top\">\n            <template slot=\"title\">\n              <span>拖拽调整顺序</span>\n            </template>\n            <div>\n              <st-icon size=\"16px\" class=\"tooltip-draggable\" type=\"tuozhuai\" />\n            </div>\n          </a-tooltip>\n        </div>\n      </st-draggable>\n    </st-form>\n  </div>\n</template>\n<script>\nimport { imgFilter } from '@/filters/resource.filters'\n\nexport default {\n  name: 'ProductSelect',\n  bem: {\n    module: 'h5-form-component'\n  },\n  props: {\n    fieldName: {\n      type: String, //字段名称,\n      required: true\n    },\n    title: {\n      type: String,\n      default: '选择活动'\n    },\n    desc: {\n      type: String,\n      default: ''\n    },\n    placeholder: {\n      type: String,\n      default: '选择活动'\n    },\n    maxNum: {\n      type: Number\n    },\n    value: {\n      type: Object,\n      default() {\n        return {}\n      }\n    }\n  },\n  methods: {\n    imgFilter,\n    getViewName(item) {\n      return item[this.fieldName]\n    },\n    openModal() {\n      this.$emit('open')\n    },\n    delectItem(index) {\n      this.$emit('delete', index)\n    },\n    onChange() {\n      this.$emit('drag')\n    }\n  }\n}\n</script>\n"]}]}