f=\"auth.get\">\n    <st-t2>{{ $c('coach') }}等级</st-t2>\n    <div class=\"mg-t24 st-des\">\n      已新增{{ resData.total }}个，支持新增{{ resData.max }}个\n    </div>\n    <st-form-table class=\"mg-t8\">\n      <thead>\n        <tr>\n          <th>{{ $c('coach') }}等级</th>\n          <th>关联{{ $c('coach') }}数</th>\n          <th>关联薪资模板数</th>\n          <th v-if=\"isShowPricingNum\">关联课程定价数</th>\n          <th>创建人</th>\n          <th>最后修改时间</th>\n          <th>操作</th>\n        </tr>\n      </thead>\n      <tbody>\n        <tr v-if=\"auth.add\">\n          <td :colspan=\"isShowPricingNum ? 7 : 6\" class=\"st-form-table__add\">\n            <a\n              v-modal-link=\"{\n                name: 'coach-level-add',\n                on: {\n                  change: onListChange\n                }\n              }\"\n            >\n              <st-button\n                type=\"dashed\"\n                icon=\"add\"\n                block\n                :disabled=\"resData.total >= resData.max\"\n              >\n                新增{{ $c('coach') }}等级\n              </st-button>\n            </a>\n          </td>\n        </tr>\n        <tr v-for=\"(item, index) in resData.list\" :key=\"index\">\n          <td>{{ item.setting_name }}</td>\n          <td>{{ item.used_number }}</td>\n          <td>{{ item.salary_template }}</td>\n          <td v-if=\"isShowPricingNum\">{{ item.class_price }}</td>\n          <td>{{ item.operator_name }}</td>\n          <td>{{ item.updated_time }}</td>\n          <td>\n            <st-table-actions>\n              <a\n                v-if=\"item.auth['brand:setting:coach_level|edit']\"\n                v-modal-link=\"{\n                  name: 'coach-level-edit',\n                  props: {\n                    id: item.id,\n                    setting_name: item.setting_name\n                  },\n                  on: {\n                    change: onListChange\n                  }\n                }\"\n              >\n                编辑\n              </a>\n              <st-popconfirm\n                v-if=\"item.auth['brand:setting:coach_level|del']\"\n                @confirm=\"onDelete(item.id)\"\n              >\n                <div slot=\"title\">\n                  <div v-if=\"item.class_price === 0\">\n                    删除后不可进行恢复,确定删除此{{ $c('coach') }}等级？\n                  </div>\n                  <div>\n                    删除{{ $c('coach') }}等级{{ item.setting_name }}\n                    <div class=\"color-danger\">\n                      当前有\n                      {{ item.class_price }}\n                      条私教课程定价使用了此{{ $c('coach') }}等级,将同时删除\n                      {{ item.class_price }} 条课程定价,请谨慎操作？\n                    </div>\n                  </div>\n                </div>\n                <a>删除</a>\n              </st-popconfirm>\n            </st-table-actions>\n          </td>\n        </tr>\n      </tbody>\n    </st-form-table>\n  </st-panel>\n</template>\n<script>\nimport { CoachLevelService } from './coach-level.service'\nimport { MessageService } from '@/services/message.service'\nimport CoachLevelEdit from '@/views/biz-modals/coach-level/edit.vue'\nimport CoachLevelAdd from '@/views/biz-modals/coach-level/add.vue'\n\nexport default {\n  serviceInject() {\n    return {\n      listService: CoachLevelService,\n      messageService: MessageService\n    }\n  },\n  rxState() {\n    return {\n      resData: this.listService.resData$,\n      auth: this.listService.auth$\n    }\n  },\n  modals: {\n    CoachLevelAdd,\n    CoachLevelEdit\n  },\n  computed: {\n    isShowPricingNum() {\n      return this.resData.price_setting === 2\n    }\n  },\n  methods: {\n    onDelete(id) {\n      this.listService.deleteCoachLevel({ id }).subscribe(() => {\n        this.messageService.success({\n          content: '删除成功'\n        })\n        this.onListChange()\n      })\n    },\n    onTableChange(pagination) {\n      this.$router.push({\n        query: {\n          page: pagination.current\n        }\n      })\n    },\n    onListChange() {\n      this.$router.push({\n        query: {\n          page: 1\n        }\n      })\n    }\n  }\n}\n</script>\n"],"sourceRoot":"src/views/pages/brand/setting/general"}]}