    <a-select-option\n                v-for=\"item in codeList\"\n                :key=\"item.code_id\"\n                :value=\"item.code_id\"\n              >\n                +{{ item.phone_code }}\n              </a-select-option>\n            </a-select>\n          </a-input>\n        </st-form-item>\n        <st-form-item label=\"性别\" required>\n          <a-radio-group name=\"radioGroup\" v-decorator=\"decorators.sex\">\n            <a-radio :value=\"SEX.MALE\">\n              男\n              <st-icon\n                class=\"sex__male\"\n                style=\"color: #636aec\"\n                type=\"male\"\n              ></st-icon>\n            </a-radio>\n            <a-radio :value=\"SEX.FEMALE\">\n              女\n              <st-icon\n                calss=\"sex__female\"\n                style=\"color: #fa756c\"\n                type=\"female\"\n              ></st-icon>\n            </a-radio>\n          </a-radio-group>\n        </st-form-item>\n      </a-col>\n      <a-col :lg=\"10\" :xs=\"22\" :offset=\"1\">\n        <st-form-item label=\"员工人脸\">\n          <face-upload\n            width=\"164px\"\n            height=\"164px\"\n            :list=\"faceList\"\n            @change=\"onChangeGetFace\"\n            placeholder=\"上传头像\"\n          ></face-upload>\n        </st-form-item>\n        <st-form-item required>\n          <template slot=\"label\">\n            昵称\n            <st-help-tooltip id=\"TSCE001\" />\n          </template>\n          <a-input\n            placeholder=\"支持中英文、数字,不超过10个字\"\n            v-decorator=\"decorators.nickname\"\n          />\n        </st-form-item>\n        <st-form-item label=\"邮箱\">\n          <a-input placeholder=\"请输入邮箱\" v-decorator=\"decorators.mail\" />\n        </st-form-item>\n        <st-form-item label=\"证件\">\n          <a-input placeholder=\"请输入\" v-decorator=\"decorators.id_number\">\n            <a-select\n              slot=\"addonBefore\"\n              style=\"width: 100px;\"\n       