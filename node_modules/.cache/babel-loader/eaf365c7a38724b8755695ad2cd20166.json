tService.page$\n    };\n  },\n  computed: {\n    columns: columns\n  },\n  modals: {\n    TaskDetail: TaskDetail\n  },\n  data: function data() {\n    return {\n      TASK_STATUS: TASK_STATUS\n    };\n  },\n  methods: {\n    refresh: function refresh() {\n      return this.listService.getList(this.$searchQuery);\n    },\n    onClickModal: function onClickModal(record, type) {\n      this.$modalRouter.push({\n        name: 'task-detail',\n        props: {\n          task_id: record.id,\n          task_type: record.task_type.id,\n          popup_type: type\n        },\n        on: {}\n      });\n    },\n    stopTask: function stopTask(item) {\n      var _this = this;\n\n      this.listService.stopTask(item.id).subscribe(function (res) {\n        _this.messageService.success({\n          content: '终止任务成功!'\n        });\n\n        _this.$router.reload();\n      });\n    }\n  }\n};",{"version":3,"sources":["list.vue"],"names":[],"mappings":";;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;AAoEA,SAAA,WAAA;AACA,SAAA,OAAA;AACA,OAAA,UAAA,MAAA,sBAAA;AACA,SAAA,WAAA,QAAA,gCAAA;AACA,OAAA,UAAA,MAAA,gCAAA;AACA,SAAA,cAAA,QAAA,4BAAA;AACA,eAAA;AACA,EAAA,MAAA,EAAA,CAAA,UAAA,CADA;AAEA,EAAA,IAAA,EAAA,0BAFA;AAGA,EAAA,aAHA,2BAGA;AACA,WAAA;AACA,MAAA,cAAA,EAAA,cADA;AAEA,MAAA,WAAA,EAAA;AAFA,KAAA;AAIA,GARA;AASA,EAAA,OATA,qBASA;AACA,WAAA;AACA,MAAA,OAAA,EAAA,KAAA,WAAA,CAAA,QADA;AAEA,MAAA,iBAAA,EAAA,KAAA,WAAA,CAAA,kBAFA;AAGA,MAAA,IAAA,EAAA,KAAA,WAAA,CAAA,KAHA;AAIA,MAAA,IAAA,EAAA,KAAA,WAAA,CAAA;AAJA,KAAA;AAMA,GAhBA;AAiBA,EAAA,QAAA,EAAA;AACA,IAAA,OAAA,EAAA;AADA,GAjBA;AAoBA,EAAA,MAAA,EAAA;AACA,IAAA,UAAA,EAAA;AADA,GApBA;AAuBA,EAAA,IAvBA,kBAuBA;AACA,WAAA;AACA,MAAA,WAAA,EAAA;AADA,KAAA;AAGA,GA3BA;AA4BA,EAAA,OAAA,EAAA;AACA,IAAA,OADA,qBACA;AACA,aAAA,KAAA,WAAA,CAAA,OAAA,CAAA,KAAA,YAAA,CAAA;AACA,KAHA;AAIA,IAAA,YAJA,wBAIA,MAJA,EAIA,IAJA,EAIA;AACA,WAAA,YAAA,CAAA,IAAA,CAAA;AACA,QAAA,IAAA,EAAA,aADA;AAEA,QAAA,KAAA,EAAA;AACA,UAAA,OAAA,EAAA,MAAA,CAAA,EADA;AAEA,UAAA,SAAA,EAAA,MAAA,CAAA,SAAA,CAAA,