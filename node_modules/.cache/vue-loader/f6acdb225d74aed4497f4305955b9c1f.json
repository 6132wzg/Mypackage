show: false,\n    }\n  },\n  computed: {\n    columns,\n  },\n  mounted() {\n    this.init()\n  },\n  methods: {\n    getList() {\n      console.log('我是新增的限时折扣')\n      this.service.getGroupBuyList(this.query).subscribe()\n    },\n    onSubmit() {\n      const data = {\n        selected: this.selectedRowKeys,\n        selectedRows: this.selectedRows || []\n      }\n      if(this.handlerValidate && !this.handlerValidate(data)) return\n      this.show = false\n      console.log('complete', data)\n      this.handlerSubmit && this.handlerSubmit(data)\n      this.$emit('complete', data)\n    },\n    onCancel() {\n      this.show = false\n    },\n    handlerValidate() {\n      if (this.selectedRowKeys.length > this.max) {\n        this.messageService.error({\n          content: '当前数量已达上限，请在删除后重新添加'\n        })\n        return false\n      }\n      return true\n    },\n  }\n}\n",{"version":3,"sources":["index.vue"],"names":[],"mappings":";;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;AAgEA;AACA;AACA;AACA;;AAEA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;;AAEA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;;AAEA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA","file":"index.vue","sourceRoot":"src/views/biz-modals/custom-decorate/discount-table-select","sourcesContent":["<template>\n  <st-modal\n    :title=\"title\"\n    v-model=\"show\"\n    @ok=\"onSubmit\"\n    @cancel=\"onCancel\"\n  >\n    <div :class=\"b()\">\n      <div :class=\"b('header')\">\n        <div :class=\"b('header-warn')\" v-if=\"selectedRowKeys.length > max\">\n          最多选择{{ max }}项\n        </div>\n        <div :class=\"b('header-select')\" v-else>\n          已选{{ selectedRowKeys.length }}项\n        </div>\n        <div :class=\"b('header-filter')\">\n          <!-- 优惠券 -->\n          <a-select\n            class=\"mg-r16\"\n            style=\"width: 145px\"\n            @change=\"onSingleSearch('type', $event)\"\n          >\n            <a-select-option :value=\"-1\">全部优惠券</a-select-option>\n            <a-select-option\n              v-for=\"item in cocach_type\"\n              :value=\"item.value\"\n              :key=\"item.value\"\n            >\n              {{ item.label }}\n            </a-select-option>\n          </a-select>\n          <st-input-search\n            style=\"width: 240px\"\n            :class=\"b('search')\"\n            @search=\"onSingleSearch('keywords', $event)\"\n            placeholder=\"搜索\"\n          ></st-input-search>\n        </div>\n      </div>\n      <div :class=\"b('body')\">\n        <st-table\n          rowKey=\"activity_id\"\n          :loading=\"loading.getGroupBuyList\"\n          :columns=\"columns\"\n          :dataSource=\"list\"\n          :page=\"page\"\n          :rowSelection=\"{\n            selectedRowKeys: selectedRowKeys,\n            onChange: onSelectionChange\n          }\"\n          :scroll=\"{ x: 700 }\"\n          :simplePage=\"true\"\n          @change=\"onTableChange\"\n        >\n          <template slot=\"groupbuy_activity_status\" slot-scope=\"text, record\">\n            {{ record.activity_state.name }}\n          </template>\n        </st-table>\n      </div>\n    </div>\n  </st-modal>\n</template>\n\n<script>\nimport { GroupBuycolumns as columns } from '../product-table-select.config'\nimport { ProductTableSelectService } from '../product-table-select.service'\nimport { MessageService } from '@/services/message.service'\nimport tableMixin from '@/mixins/table2.mixin'\n\nexport default {\n  mixins: [tableMixin],\n  bem: {\n    b: 'custom-decorate-product-table-select'\n  },\n  props: {\n    title: {\n      type: String\n    },\n    /**\n     * 最大选择数量\n     */\n    max: {\n      type: Number,\n      default: 10\n    },\n  },\n\n  serviceInject() {\n    return {\n      messageService: MessageService,\n      service: ProductTableSelectService\n    }\n  },\n  rxState() {\n    return {\n      loading: this.service.loading$,\n      list: this.service.list$,\n      page: this.service.page$,\n      cocach_type: this.service.cocach_type$\n    }\n  },\n\n  data() {\n    return {\n      show: false,\n    }\n  },\n  computed: {\n    columns,\n  },\n  mounted() {\n    this.init()\n  },\n  methods: {\n    getList() {\n      console.log('我是新增的限时折扣')\n      this.service.getGroupBuyList(this.query).subscribe()\n    },\n    onSubmit() {\n      const data = {\n        selected: this.selectedRowKeys,\n        selectedRows: this.selectedRows || []\n      }\n      if(this.handlerValidate && !this.handlerValidate(data)) return\n      this.show = false\n      console.log('complete', data)\n      this.handlerSubmit && this.handlerSubmit(data)\n      this.$emit('complete', data)\n    },\n    onCancel() {\n      this.show = false\n    },\n    handlerValidate() {\n      if (this.selectedRowKeys.length > this.max) {\n        this.messageService.error({\n          content: '当前数量已达上限，请在删除后重新添加'\n        })\n        return false\n      }\n      return true\n    },\n  }\n}\n</script>\n"]}]}