class=\"module('rule-label')\">显示规则</div>\n      <a-radio-group\n        :options=\"recommendRuleOptions\"\n        v-model=\"data_source\"\n        @change=\"onRecommendRuleChange\"\n      ></a-radio-group>\n    </div>\n    <div v-if=\"isShowCustom\" :class=\"module('package-list')\">\n      <div :class=\"module('package-label')\">选择课程包</div>\n      <div :class=\"module('package-desc')\">\n        拖动课程包可进行排序\n      </div>\n      <a-alert\n        class=\"mg-t16\"\n        v-if=\"value.list.length > maxNum - 1\"\n        message=\"当前数量已达上限，可在删除后重新添加\"\n        banner\n      ></a-alert>\n      <st-form class=\"mg-t16\">\n        <div\n          :class=\"module('box')\"\n          class=\"h5-form-package-component__add\"\n          @click=\"onCustomAction\"\n          v-if=\"value.list.length < maxNum\"\n        >\n          <st-icon type=\"add\" size=\"14px\" color=\"rgba(62, 77, 92, 1)\" />\n          <div :class=\"module('add-label')\">选择课程包</div>\n        </div>\n        <st-draggable\n          v-model=\"value.list\"\n          :animation=\"200\"\n          :class=\"module('draggable-wrap')\"\n          @change=\"onChange\"\n        >\n          <div\n            v-for=\"(item, index) in value.list\"\n            :key=\"index\"\n            :class=\"module('box')\"\n            style=\"cursor: move\"\n          >\n            <div :class=\"module('del')\" @click=\"onDel(index)\">\n              <st-icon type=\"delete\" color=\"#FF5E41\" size=\"16px\" />\n            </div>\n            <img\n              :class=\"module('bg')\"\n              style=\"object-fit: cover;\"\n              :src=\"imgFilter(item.image_url, { w: 180, h: 100 })\"\n              width=\"225\"\n              height=\"126\"\n            />\n            <div :class=\"module('name')\" class=\"text-overflow\">\n              {{ item.package_name }}\n            </div>\n            <a-tooltip :class=\"module('draggable-icon')\" placement=\"top\">\n              <template slot=\"title\">\n                <span>拖拽调整顺序</span>\n              </template>\n              <div>\n                <st-icon\n                  size=\"16px\"\n                  class=\"tooltip-draggable\"\n                  type=\"tuozhuai\"\n                />\n              </div>\n            </a-tooltip>\n          </div>\n        </st-draggable>\n      </st-form>\n    </div>\n  </div>\n</template>\n<script>\nimport mixin from './mixin'\nimport { H5FormPackageComponentService } from './h5-form.package.component.service'\nimport { imgFilter } from '@/filters/resource.filters'\n\nexport default {\n  name: 'h5FormPackageComponent',\n  mixins: [mixin],\n  bem: {\n    module: 'h5-form-package-component'\n  },\n  serviceInject() {\n    return {\n      packageService: H5FormPackageComponentService\n    }\n  },\n  rxState() {\n    const { directionOptions$, recommendRuleOptions$ } = this.packageService\n    return {\n      directionOptions: directionOptions$,\n      recommendRuleOptions: recommendRuleOptions$\n    }\n  },\n  data() {\n    return {\n      modalConfig: {\n        title: '选择课程包',\n        max: this.maxNum\n      },\n      show_direction: 1,\n      data_source: null\n    }\n  },\n  methods: {\n    imgFilter,\n    adaptor(selectedRows) {\n      return selectedRows.map(item => {\n        return {\n          id: item.id,\n          package_name: item.course_name,\n          times: item.times,\n          price: item.price,\n          tag: item.tag,\n          image_url: item.image_url\n        }\n      })\n    },\n    onRecommendRuleChange() {\n      this.getPreview()\n      this.onChange()\n    },\n    getPreview() {\n      if (this.isShowCustom) {\n        return\n      }\n      const query = {\n        shop_id: this.shopId,\n        module_type: this.moduleType,\n        data_source: this.data_source\n      }\n      this.packageService.getPreview(query).subscribe(res => {\n        this.value.list = res.list\n      })\n    }\n  }\n}\n</script>\n"]}]}