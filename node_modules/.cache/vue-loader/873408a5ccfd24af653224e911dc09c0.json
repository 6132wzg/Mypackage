ctivityInfo$,\n      ticketTypeOptions$\n    }\n  },\n  computed: {\n    columns\n  },\n  methods: {\n    onClickShowInfo(record) {\n      this.$modalRouter.push({\n        name: 'marketing-activity-sign-up-info',\n        props: {\n          signUpInfo: record\n        }\n      })\n    },\n    onConfirmSignIn(record) {\n      this.service.updateSignUpChecked(record.id).subscribe(res => {\n        this.$router.reload()\n      })\n    }\n  }\n}\n",{"version":3,"sources":["roster.vue"],"names":[],"mappings":";;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;AAyFA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA","file":"roster.vue","sourceRoot":"src/views/pages/brand/marketing/plugin/activity-registration","sourcesContent":["<template>\n  <div :class=\"bPage()\">\n    <div :class=\"bPage('info')\">\n      <st-t3 class=\"title\">{{ activityInfo$.activity_name }}</st-t3>\n      <div class=\"time mg-t24\">\n        <span class=\"label\">活动时间：</span>\n        <span class=\"value\">\n          {{ activityInfo$.start_time }}-{{ activityInfo$.end_time }}\n        </span>\n      </div>\n      <div class=\"address mg-t8\">\n        <span class=\"label\">活动地点：</span>\n        <span class=\"value\">{{ activityInfo$.address }}</span>\n      </div>\n    </div>\n    <st-panel class=\"mg-t24\" app>\n      <div :class=\"bPage('search')\">\n        <div :class=\"bSearch('select-group')\">\n          <a-select\n            placeholder=\"请选择状态\"\n            @change=\"onSingleSearch('ticket_status', $event)\"\n            class=\"mg-r16\"\n            v-model=\"$searchQuery.ticket_status\"\n            style=\"width: 160px\"\n            :options=\"ticketStatus$\"\n          ></a-select>\n          <a-select\n            placeholder=\"请选择票种\"\n            @change=\"onSingleSearch('ticket_id', $event)\"\n            class=\"mg-r16\"\n            v-model=\"$searchQuery.ticket_id\"\n            style=\"width: 160px\"\n            :options=\"ticketTypeOptions$\"\n          ></a-select>\n        </div>\n        <div :class=\"bSearch('input-group')\">\n          <st-input-search\n            @search=\"onSingleSearch('keyword', $event)\"\n            placeholder=\"请输入姓名、手机号\"\n            style=\"width: 290px;\"\n          />\n        </div>\n      </div>\n      <div :class=\"bPage('list')\" class=\"mg-t16\">\n        <st-table\n          :page=\"page$\"\n          rowKey=\"id\"\n          :columns=\"columns\"\n          @change=\"onTableChange\"\n          :scroll=\"{ x: 1500 }\"\n          :dataSource=\"list$\"\n        >\n          <template slot=\"ticket_status\" slot-scope=\"text\">\n            <st-text\n              :status=\"{\n                error: text.id === 1,\n                success: text.id === 2,\n                default: text.id === 3\n              }\"\n            >\n              {{ text.name }}\n            </st-text>\n          </template>\n          <div slot=\"registration_info\" slot-scope=\"text\">\n            <span v-if=\"!Array.isArray(text)\">--</span>\n            <a v-else @click=\"onClickShowInfo(text)\">查看详情</a>\n          </div>\n          <template slot=\"action\" slot-scope=\"text, record\">\n            <st-table-actions>\n              <a v-if=\"record.ticket_status.id === 1\">\n                <st-popconfirm\n                  title=\"签到后将视为已使用，您确定要将其核销签到吗？\"\n                  @confirm=\"onConfirmSignIn(record)\"\n                  okText=\"确认\"\n                  cancelText=\"取消\"\n                >\n                  签到\n                </st-popconfirm>\n              </a>\n              <span v-else>{{ record.ticket_status.name }}</span>\n            </st-table-actions>\n          </template>\n        </st-table>\n      </div>\n    </st-panel>\n  </div>\n</template>\n\n<script>\nimport tableMixin from '@/mixins/table.mixin'\nimport { columns } from './roster.config'\nimport { RosterService } from './roster.service'\nimport MarketingActivitySignUpInfo from '@/views/biz-modals/marketing/activity-signup-info'\nexport default {\n  name: 'RosterList',\n  mixins: [tableMixin],\n  bem: {\n    bPage: 'marketing-plugin-activity-roster',\n    bSearch: 'roster-search'\n  },\n  modals: {\n    MarketingActivitySignUpInfo\n  },\n  serviceInject() {\n    return {\n      service: RosterService\n    }\n  },\n  rxState() {\n    const {\n      list$,\n      page$,\n      loading$,\n      ticketStatus$,\n      activityInfo$,\n      ticketTypeOptions$\n    } = this.service\n    return {\n      list$,\n      page$,\n      loading$,\n      ticketStatus$,\n      activityInfo$,\n      ticketTypeOptions$\n    }\n  },\n  computed: {\n    columns\n  },\n  methods: {\n    onClickShowInfo(record) {\n      this.$modalRouter.push({\n        name: 'marketing-activity-sign-up-info',\n        props: {\n          signUpInfo: record\n        }\n      })\n    },\n    onConfirmSignIn(record) {\n      this.service.updateSignUpChecked(record.id).subscribe(res => {\n        this.$router.reload()\n      })\n    }\n  }\n}\n</script>\n"]}]}