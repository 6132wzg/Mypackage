 :class=\"b()\">\n    <MarkteingPluginTitle pathName=\"brand-app-plugin-seckill-index-list\" />\n    <div :class=\"b('intro')\" v-if=\"info.is_open\">\n      <img\n        :class=\"b('intro-item')\"\n        src=\"https://styd-frontend.oss-cn-shanghai.aliyuncs.com/images/saas-pro/plugin_pos_banner_1%402x.png\"\n        alt=\"\"\n      />\n      <img\n        :class=\"b('intro-item')\"\n        src=\"https://styd-frontend.oss-cn-shanghai.aliyuncs.com/images/saas-pro/plugin_pos_banner_2%402x.png\"\n        alt=\"\"\n      />\n      <img\n        :class=\"b('intro-item')\"\n        src=\"https://styd-frontend.oss-cn-shanghai.aliyuncs.com/images/saas-pro/plugin_pos_banner_3%402x.png\"\n        alt=\"\"\n      />\n      <img\n        :class=\"b('intro-item')\"\n        src=\"https://styd-frontend.oss-cn-shanghai.aliyuncs.com/images/saas-pro/plugin_pos_banner_4%402x.png\"\n        alt=\"\"\n      />\n    </div>\n    <st-panel v-else app>\n      <header :class=\"bHeader('header')\">\n        <div :class=\"bHeader('button-wapper')\">\n          <st-button type=\"primary\" icon=\"add\" @click=\"onAdd\">\n            新增活动\n          </st-button>\n          <st-button @click=\"onConfigHandle\">配置到小程序</st-button>\n        </div>\n        <div :class=\"bHeader('actions')\">\n          <st-select\n            v-model=\"$searchQuery.product_type\"\n            :options=\"productType\"\n            @change=\"onSingleSearch('product_type', $event)\"\n          ></st-select>\n          <st-select\n            v-model=\"$searchQuery.activity_status\"\n            :options=\"activityStatus\"\n            @change=\"onSingleSearch('activity_status', $event)\"\n          ></st-select>\n          <st-input-search\n            v-model=\"$searchQuery.keywords\"\n            placeholder=\"搜素活动名称\"\n            style=\"width: 300px\"\n            @search=\"onSingleSearch('keywords', $event)\"\n          ></st-input-search>\n        </div>\n      </header>\n      <div :class=\"b('main')\">\n        <st-table\n          :rowKey=\"(record, idx) => idx\"\n          :columns=\"columns\"\n          :page=\"page\"\n          :dataSource=\"list\"\n          :loading=\"loading.getList\"\n          @change=\"onTableChange\"\n        >\n          <span slot=\"product_type\" slot-scope=\"text\">\n            {{ text | enumFilter('limited_time_discount.product_type') }}\n          </span>\n          <span slot=\"temp\" slot-scope=\"text, record\">\n            {{ record.start_time + '~' + record.end_time }}\n          </span>\n          <span slot=\"activity_status\" slot-scope=\"text\">\n            {{ text | enumFilter('limited_time_discount.activity_status') }}\n          </span>\n          <a\n            slot=\"order_num\"\n            slot-scope=\"text, record\"\n            v-modal-link=\"{\n              name: 'order-list-modal',\n              props: {\n                query: {\n                  ...$searchQuery,\n                  id: record.activity_id\n                },\n                title: '订单明细',\n                type: 'brand-plugin-seckill'\n              }\n            }\"\n          >\n            {{ text }}\n          </a>\n          <span slot=\"operator\" slot-scope=\"text, record\">\n            <st-table-actions sytle=\"width: 120px\">\n              <a\n                @click=\"onEdit(record)\"\n                v-if=\"record.auth['brand:activity:limit_time_spike|edit']\"\n              >\n                编辑\n              </a>\n              <a\n                @click=\"onStop(record)\"\n                v-if=\"record.auth['brand:activity:limit_time_spike|stop']\"\n              >\n                结束\n              </a>\n              <a\n                @click=\"onDelete(record)\"\n                v-if=\"record.auth['brand:activity:limit_time_spike|delete']\"\n              >\n                删除\n              </a>\n            </st-table-actions>\n          </span>\n        </st-table>\n      </div>\n    </st-panel>\n  </st-panel-layout>\n</template>\n<script>\nimport OrderListModal from '@/views/biz-modals/stat/order-detail'\nimport MarkteingPluginTitle from '@/views/pages/brand/marketing/components#/marketing-title'\nimport tableMixin from '@/mixins/table.mixin'\nimport { columns } from './list.config'\nimport { ListService } from './list.service'\n\nexport default {\n  name: 'PluginLotteryIndex',\n  mixins: [tableMixin],\n  modals: {\n    OrderListModal\n  },\n  components: {\n    MarkteingPluginTitle\n  },\n  data() {\n    return {}\n  },\n  bem: {\n    b: 'page-brand-app-plugin-discount',\n    bHeader: 'page-header'\n  },\n  serviceInject() {\n    return {\n      service: ListService\n    }\n  },\n  rxState() {\n    return {\n      productType: this.service.productType$,\n      activityStatus: this.service.activityStatus$,\n      loading: this.service.loading$,\n      page: this.service.page$,\n      list: this.service.list$,\n      info: this.service.info$\n    }\n  },\n  computed: {\n    columns\n  },\n  created() {},\n  methods: {\n    onAdd() {\n      this.$router.push({\n        path: './add'\n      })\n    },\n    onEdit(record) {\n      this.$router.push({\n        path: './edit',\n        query: { id: record.activity_id }\n      })\n    },\n    onStop(record) {\n      let that = this\n      this.$confirm({\n        title: '提示',\n        content: '确定停止该活动吗？',\n        onOk() {\n          that.service.stopSeckill(record.activity_id).subscribe(res => {\n            that.$router.reload()\n          })\n        },\n        onCancel() {}\n      })\n    },\n    onDelete(record) {\n      let that = this\n      this.$confirm({\n        title: '提示',\n        content: '确定删除该活动吗？',\n        onOk() {\n          that.service.deleteSeckill(record.activity_id).subscribe(res => {\n            that.$router.reload()\n          })\n        },\n        onCancel() {}\n      })\n    },\n    onConfigHandle() {\n      this.$router.push({\n        path: '/brand/setting/mina/index'\n      })\n    }\n  }\n}\n</script>\n"]}]}