         <div :class=\"bItem('footer')\">\n            <template v-if=\"isInitial(item)\">\n              <!-- <router-link :class=\"bItem('action')\" to=\"/\" class=\"st-link-secondary\">查看</router-link>\n              <div :class=\"bItem('divider')\"></div> -->\n              <router-link\n                v-if=\"auth.edit\"\n                :class=\"bItem('action')\"\n                :to=\"{ path: './edit', query: { id: item.id } }\"\n                class=\"st-link-secondary\"\n              >\n                编辑\n              </router-link>\n            </template>\n            <template v-else>\n              <router-link\n                v-if=\"auth.edit\"\n                :class=\"bItem('action')\"\n                :to=\"{ path: './edit', query: { id: item.id } }\"\n                class=\"st-link-secondary\"\n              >\n                去设置\n              </router-link>\n            </template>\n          </div>\n        </div>\n      </a-col>\n    </a-row>\n    <h3 v-if=\"!list.length\">无任何合同模版</h3>\n  </st-panel>\n</template>\n<script>\nimport { ListService } from './list.service'\nimport { UserService } from '@/services/user.service'\nexport default {\n  bem: {\n    bPage: 'page-brand-setting-contract-list',\n    bItem: 'contract-item'\n  },\n  serviceInject() {\n    return {\n      listService: ListService,\n      userService: UserService\n    }\n  },\n  rxState() {\n    /**\n     * @type {UserService}\n     */\n    const user = this.userService\n    return {\n      list: this.listService.list$,\n      auth: this.listService.auth$\n    }\n  },\n  methods: {\n    isInitial(item) {\n      return !!item.is_initialize_status\n    }\n  }\n}\n</script>\n"],"sourceRoot":"src/views/pages/brand/setting/contract"}]}