 {\n    columns\n  },\n  created() {\n    this.getList(this.page)\n  },\n  methods: {\n    getList(page) {\n      const { current_page, size } = page\n      this.punishService\n        .getList({\n          size,\n          current_page\n        })\n        .subscribe()\n    },\n    delPunishment(id) {\n      this.punishService.delPunishment(id).subscribe(() => {\n        this.messageService.success({\n          content: '解除惩罚成功！'\n        })\n        this.getList({\n          ...this.page,\n          current_page: 1\n        })\n      })\n    },\n    onPageChange(page) {\n      this.getList({\n        current_page: page.current,\n        size: page.pageSize\n      })\n    }\n  }\n}\n</script>\n"]}]}