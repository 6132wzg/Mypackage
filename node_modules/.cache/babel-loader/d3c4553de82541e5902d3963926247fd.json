   @change=\"pertimeChange\"\n        >\n          <a-radio\n            v-for=\"item in timeEnums\"\n            :key=\"item.value\"\n            :value=\"item.value\"\n          >\n            {{ item.label }}\n          </a-radio>\n        </a-radio-group>\n      </st-form-item>\n      <st-form-item :class=\"b('time')\" label=\"可预约时间\" required>\n        <st-form-item :class=\"b('inline-item')\">\n          <a-select\n            v-decorator=\"decorators.open_time_start\"\n            placeholder=\"开始时间\"\n            class=\"page-content-card-input\"\n          >\n            <a-select-option\n              v-for=\"(item, index) in pertimeEnums[perTime]\"\n              :key=\"index\"\n              :value=\"item.value\"\n            >\n              {{ item.label }}\n            </a-select-option>\n          </a-select>\n        </st-form-item>\n        &nbsp;~&nbsp;\n        <st-form-item :class=\"b('inline-item')\">\n          <a-select\n            v-decorator=\"decorators.open_time_end\"\n            placeholder=\"结束时间\"\n            class=\"page-content-card-input\"\n          >\n            <a-select-option\n              v-for=\"(item, index) in pertimeEnums[perTime]\"\n              :key=\"index\"\n              :value=\"item.value\"\n            >\n              {{ item.label }}\n            </a-select-option>\n          </a-select>\n        </st-form-item>\n      </st-form-item>\n      <st-form-item label=\"最少预约场次\">\n        每次最少连续预约\n        <st-input-number\n          :class=\"b('inline')\"\n          v-decorator=\"decorators.min_reserve_num\"\n          placeholder=\"请输入数量\"\n        >\n          <template slot=\"addonAfter\">\n            场\n          </template>\n        </st-input-number>\n      </st-form-item>\n      <st-form-item label=\"最多预约场次\">\n        每个人一天最多可预约\n        <st-input-number\n          :class=\"b('inline')\"\n          v-decorator=\"decorators.max_reserve_num\"\n          placeholder=\"请输入数量\"\n        >\n          <template slot=\"addonAfter\">\n            场\n          </template>\n        </st-input-number>\n      </st-form-item>\n      <st-form-item label=\"默认价格\" required>\n        <st-input-number\n          v-decorator=\"decorators.price\"\n          :float=\"true\"\n          placeholder=\"请输入价格\"\n        >\n          <template slot=\"addonAfter\">\n            元/场\n          </template>\n        </st-input-number>\n      </st-form-item>\n      <st-form-item label=\"预约说明\">\n        <st-textarea\n          v-decorator=\"decorators.description\"\n          style=\"width: 100%;\"\n          :float=\"true\"\n          :maxlength=\"50\"\n          :autosize=\"{\n            minRows: 1,\n            maxRow: 3\n          }\"\n          placeholder=\"请输入预约说明\"\n        ></st-textarea>\n      </st-form-item>\n    </st-form>\n    <div :class=\"b('tips')\">\n      <span>用户在小程序预约付款时展示</span>\n      <a\n        v-modal-link=\"{\n          name: 'shop-app-venue-view-image',\n          props: {\n            url:\n              'https://styd-frontend.oss-cn-shanghai.aliyuncs.com/images/saas-pro/venue_desc_preview.png'\n          }\n        }\"\n      >\n        &nbsp;示例\n      </a>\n    </div>\n    <div :class=\"b('btn')\">\n      <st-button type=\"primary\" @click=\"onClickSubmit\" class=\"mg-r16\">\n        保存\n      </st-button>\n    </div>\n  </div>\n</template>\n\n<script>\nimport { cloneDeep } from 'lodash-es'\nimport { EditService } from './edit.service'\nimport { ruleOptions } from './add.config'\nimport { MessageService } from '@/services/message.service'\nimport { PatternService } from '@/services/pattern.service'\nimport ShopAppVenueViewImage from '@/views/biz-modals/shop/app/venue/view-image'\nexport default {\n  name: 'AddRole',\n  bem: {\n    b: 'page-shop-app-venue-manage-edit'\n  },\n  serviceInject() {\n    return {\n      editService: EditService,\n      messageService: MessageService,\n      pattern: PatternService\n    }\n  },\n  rxState() {\n    return {\n      timeEnums: this.editService.timeEnums$,\n      harfEnums: this.editService.harfEnums$,\n      oneEnums: this.editService.oneEnums$,\n      info: this.editService.info$\n    }\n  },\n  modals: {\n    ShopAppVenueViewImage\n  },\n  data() {\n    const form = this.$stForm.create()\n    const decorators = form.decorators(ruleOptions)\n    return {\n      form,\n      decorators,\n      perTime: 1,\n      pertimeEnums: {\n        1: this.harfEnums,\n        2: this.oneEnums\n      }\n    }\n  },\n  mounted() {\n    this.init()\n  },\n  methods: {\n    pertimeChange(e) {\n      this.perTime = e.target.value\n      this.form.setFieldsValue({\n        open_time_start: undefined,\n        open_time_end: undefined\n      })\n    },\n    init() {\n      this.form.setFieldsValue({\n        venues_name: this.info.venues_name,\n        open_time_start: this.info.open_time_start,\n        open_time_end: this.info.open_time_end,\n        per_time: this.info.per_time,\n        min_reserve_num: this.info.min_reserve_num,\n        max_reserve_num: this.info.max_reserve_num,\n        price: this.info.price,\n        description: this.info.description\n      })\n      this.perTime = this.info.per_time\n    },\n    onClickSubmit(e) {\n      e.preventDefault()\n      this.form.validate().then(values => {\n        const postData = {\n          ...values,\n          id: this.info.id\n        }\n        this.editService.edit(postData).subscribe(() => {\n          this.messageService.success({ content: '编辑成功' })\n          this.$router.push({\n            name: 'shop-app-venue-manage-list',\n            query: {\n              id: this.info.venues_id\n            }\n          })\n        })\n      })\n    }\n  }\n}\n</script>\n"],"sourceRoot":"src/views/pages/shop/app/venue/manage"}]}