OP5'\n    },\n    salesTitle: {\n      type: Array,\n      default: () => {\n        return []\n      }\n    },\n    salesList: {\n      type: [Array, Object],\n      default: () => {\n        return []\n      }\n    },\n    nameLength: {\n      type: [String, Number],\n      default: 4\n    }\n  },\n  data() {\n    return {\n      IconOneRanking,\n      IconTwoRanking,\n      IconThreeRanking\n    }\n  },\n  computed: {\n    imgIcon() {\n      let iconArr = [\n        this.IconOneRanking,\n        this.IconTwoRanking,\n        this.IconThreeRanking\n      ]\n      return iconArr\n    }\n  },\n  methods: {\n    nameFilter(value, nameLength) {\n      return value && value.length > nameLength\n        ? `${value.slice(0, nameLength)}...`\n        : value\n    },\n    thousandBit(value) {\n      let valueArr = (value + '').split('.')\n      return `${valueArr[0].replace(/\\d{1,3}(?=(\\d{3})+$)/g, '$&,')}${\n        valueArr[1] ? '.' + valueArr[1] : ''\n      }`\n    }\n  }\n}\n</script>\n"],"sourceRoot":"src/views/pages/shop/store/data/components#"}]}