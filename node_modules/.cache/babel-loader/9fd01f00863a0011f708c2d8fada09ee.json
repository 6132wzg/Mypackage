</st-form-item>\n        <st-form-item v-if=\"isShowLevel\">\n          <template slot=\"label\">\n            {{ $c('coach') }}等级\n            <st-help-tooltip id=\"TBCE003\" />\n          </template>\n          <coach-level-select\n            :placeholder=\"`请选择${$c('coach')}等级`\"\n            style=\"width: 100%\"\n            useType=\"form\"\n            v-decorator=\"decorators.coach_level_id\"\n            @change=\"onChange\"\n          ></coach-level-select>\n        </st-form-item>\n      </a-col>\n      <a-col :offset=\"1\" :lg=\"10\" :xs=\"22\">\n        <st-form-item label=\"工号\">\n          <a-input\n            placeholder=\"请输入员工工号\"\n            v-decorator=\"decorators.staff_num\"\n          ></a-input>\n        </st-form-item>\n        <st-form-item label=\"入职时间\">\n          <a-date-picker\n            style=\"width:100%\"\n            v-decorator=\"decorators.entry_date\"\n          />\n        </st-form-item>\n        <st-form-item label=\"所属门店\" required>\n          <shop-select\n            mode=\"multiple\"\n            useType=\"form\"\n            placeholder=\"选择\"\n            v-decorator=\"decorators.shop_id\"\n          />\n        </st-form-item>\n      </a-col>\n    </a-row>\n\n    <a-row :gutter=\"8\">\n      <a-col :offset=\"1\" :lg=\"22\">\n        <st-hr></st-hr>\n      </a-col>\n    </a-row>\n\n    <a-row :gutter=\"8\">\n      <a-col :offset=\"1\" :lg=\"10\">\n        <st-form-item label=\"系统权限\">\n          <a-checkbox\n            @change=\"permissionChange\"\n            v-decorator=\"decorators.is_permission\"\n          >\n            开通系统使用权限\n          </a-checkbox>\n        </st-form-item>\n        <st-form-item label=\"登录账号\" v-if=\"isChoosePermission\" required>\n          <a-input\n            placeholder=\"6-18个字符，可使用字母、数字、下划线\"\n            v-decorator=\"decorators.account\"\n          ></a-input>\n        </st-form-item>\n        <st-form-item label=\"登录密码\" v-if=\"isChoosePermission\" required>\n          <a-input\n            placeholder=\"6-15个字符，区分大小写\"\n            type=\"password\"\n            v-decorator=\"decorators.password\"\n          ></a-input>\n        </st-form-item>\n        <st-form-item label=\"确认密码\" v-if=\"isChoosePermission\" required>\n          <a-input\n            placeholder=\"请再次填写密码\"\n            type=\"password\"\n            v-decorator=\"decorators.repeat_password\"\n          ></a-input>\n        </st-form-item>\n      </a-col>\n    </a-row>\n\n    <a-row :gutter=\"8\">\n      <a-col :offset=\"1\">\n        <st-form-item labelFix>\n          <st-button @click=\"goNext\" :loading=\"loading.addStaff\" type=\"primary\">\n            保存，继续填写细信息\n          </st-button>\n        </st-form-item>\n      </a-col>\n    </a-row>\n  </st-form>\n</template>\n<script>\nimport StImageUpload from '@/views/components/image-upload#/image-upload.vue'\nimport CoachLevelSelect from '@/views/biz-components/staff/coach-level-select'\nimport { UserService } from '@/services/user.service'\nimport { AddService } from '../add.service'\nimport ShopSelect from '@/views/biz-components/shop/shop-select'\nimport DepartmentSelect from '@/views/biz-components/staff/department-select'\nimport { AppConfig } from '@/constants/config'\nimport { IDENTITY } from '@/constants/staff/info'\nimport { SEX } from '@/constants/common/form'\nimport { PatternService } from '@/services/pattern.service'\nimport { ruleOptions } from '../staff-form.config.ts'\nimport FaceUpload from '@/views/biz-components/face-upload/face-upload'\nimport { cloneDeep } from 'lodash-es'\nexport default {\n  name: 'StaffDetailBasics',\n  bem: {\n    addBasicsInfo: 'page-brand-staff-add-detail'\n  },\n  serviceInject() {\n    return {\n      pattern: PatternService,\n      appConfig: AppConfig,\n      userService: UserService,\n      addService: AddService\n    }\n  },\n  rxState() {\n    return {\n      codeList: this.addService.codeList$,\n      roleList: this.addService.roleList$,\n      loading: this.addService.loading$,\n      id_types: this.userService.getOptions$('staff.id_type'),\n      identitys: this.userService.getOptions$('staff.identity'),\n      nature_works: this.userService.getOptions$('staff.nature_work')\n    }\n  },\n  data(vm) {\n    const form = this.$stForm.create()\n    const decorators = form.decorators(ruleOptions)\n    return {\n      form,\n      decorators,\n      SEX,\n      fileList: [],\n      faceList: [],\n      isChoosePermission: false,\n      isAdd: [],\n      addflag: false,\n      isShowLevel: false, // 是否展示教练等级\n      treeExpandedKeys: [],\n      value: undefined,\n      id_type: 1,\n      country_code_id: 37\n    }\n  },\n  components: {\n    ShopSelect,\n    DepartmentSelect,\n    CoachLevelSelect,\n    FaceUpload,\n    StImageUpload\n  },\n  methods: {\n    resetID() {\n      console.log(111)\n      this.form.setFieldsValue({\n        id_number: undefined\n      })\n    },\n    onChangeGetAvatar(imageFiles) {\n      this.fileList = cloneDeep(imageFiles)\n    },\n    onChangeGetFace(imageFiles) {\n      this.faceList = cloneDeep(imageFiles)\n    },\n    getIsCoach(data) {\n      console.log('watch new', data)\n      this.isShowLevel = data.includes(IDENTITY.PARSONAL_COACH)\n\n      if (!this.isShowLevel) {\n        this.$emit('deletStep')\n        this.addflag = false\n      } else {\n        if (this.addflag) return\n        this.$emit('addStep')\n        this.addflag = true\n      }\n    },\n    permissionChange(e) {\n      this.isChoosePermission = e.target.checked\n    },\n    onChange(value) {\n      console.log('选择部门', value)\n      this.value = value\n    },\n    // 继续填写跳转到编辑\n    goNext(e) {\n      this.form.validate().then(values => {\n        this.submit(values)\n      })\n    },\n    /**\n     * saveOrgoNext 0 保存 1 跳转到编辑\n     */\n    submit(data) {\n      data.is_permission = +this.isChoosePermission\n      data.entry_date = moment(data.entry_date).format('YYYY-MM-DD')\n      data.image_avatar = this.fileList[0]\n      data.image_face = this.faceList[0]\n      data.country_code_id = this.country_code_id\n      data.id_type = this.id_type\n      this.addService.addStaff(data).subscribe(res => {\n        this.$router.push({\n          name: 'brand-staff-edit',\n          query: {\n            id: res.staff_id,\n            currentIndex: 1,\n            isShowCoach:\n              data.identity.includes(IDENTITY.TEAM_COACH) ||\n              data.identity.includes(IDENTITY.PARSONAL_COACH)\n                ? 1\n                : 0\n          }\n        })\n      })\n    }\n  }\n}\n</script>\n"],"sourceRoot":"src/views/pages/brand/staff/add#"}]}