CA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA","file":"h5-form.turntable.component.vue","sourceRoot":"src/views/pages/brand/setting/mina/components#/h5/form-components#","sourcesContent":["<template>\n  <div :class=\"module()\">\n    <div :class=\"module('title')\">\n      <div :class=\"module('title-text')\">{{ moduleName }}</div>\n    </div>\n    <div :class=\"module('content')\">\n      <div :class=\"module('label')\">选择活动</div>\n      <div :class=\"module('desc')\">\n        最多添加{{ maxNum }}个活动，点击可更换大转盘活动封面\n      </div>\n      <!-- 未选择 -->\n      <div\n        :class=\"module('add')\"\n        v-if=\"!value.list.length\"\n        @click=\"onCustomAction\"\n      >\n        <st-icon type=\"add\" size=\"14px\" color=\"rgba(62, 77, 92, 1)\" />\n        <div :class=\"module('add-label')\">选择活动</div>\n      </div>\n      <!-- 已选择 -->\n      <a-alert\n        class=\"mg-t16\"\n        v-else\n        message=\"当前数量已达上限，可在删除后重新添加\"\n        banner\n      ></a-alert>\n      <div\n        v-for=\"(item, index) in value.list\"\n        :key=\"index\"\n        :class=\"module('select')\"\n      >\n        <div :class=\"module('del')\" @click=\"onDel(index)\">\n          <st-icon type=\"delete\" color=\"#FF5E41\" size=\"16px\" />\n        </div>\n        <st-image-upload\n          :sizeLimit=\"5\"\n          :list=\"Object.keys(item.image).length ? [item.image] : []\"\n          :filterOptions=\"{ w: 750, h: 200 }\"\n          width=\"466px\"\n          height=\"124px\"\n          placeholder=\"选择活动\"\n          :cropperModal=\"{\n            title: '活动图片裁切',\n            cropper: { aspectRatio: 466 / 124 }\n          }\"\n          @change=\"onImageChange($event, index)\"\n        ></st-image-upload>\n        <div :class=\"module('name')\">{{ item.activity_name }}</div>\n      </div>\n    </div>\n  </div>\n</template>\n<script>\nimport mixin from './mixin'\nimport StImageUpload from '@/views/biz-components/st/image-upload/image-upload.vue'\nexport default {\n  name: 'h5FormTurntableComponent',\n  mixins: [mixin],\n  bem: {\n    module: 'h5-form-turntable-component'\n  },\n  components: {\n    StImageUpload\n  },\n  data() {\n    return {\n      idKeyName: 'activity_id',\n      modalConfig: {\n        title: '选择大转盘活动',\n        max: this.maxNum\n      }\n    }\n  },\n  methods: {\n    adaptor(selectedRows) {\n      return selectedRows.map(item => {\n        return {\n          activity_id: item.activity_id,\n          activity_name: item.activity_name,\n          image: item.image\n        }\n      })\n    }\n  }\n}\n</script>\n"]}]}