    change: 选择变更事件\n      refresh: 联动条件变化触发刷新\n      search: 查询关键词变化触发搜索\n      scroll: 下拉滚动，加载更多\n      *search refresh scroll 三个事件输出的参数为 { [_keyword]: '', ...query }\n    支持 suffixIcon图标插槽、notContent无数据插槽、notFoundContent查无内容插槽\n    </pre>\n    <st-select-scroll\n      style=\"width: 150px\"\n      class=\"mg-l24\"\n      _label=\"name\"\n      _value=\"id\"\n      _keyword=\"coupon_name\"\n      isSrcoll\n      v-model=\"listValue\"\n      :loading=\"isLoading\"\n      :isEnd=\"isEnd\"\n      :list=\"list\"\n      @change=\"onChange\"\n      @refresh=\"onSearch(true, $event)\"\n      @search=\"onSearch(true, $event)\"\n      @scroll=\"onSearch(false, $event)\"\n    ></st-select-scroll>\n    <st-select-scroll\n      style=\"width: 150px\"\n      class=\"mg-l24\"\n      _label=\"name\"\n      _value=\"id\"\n      _keyword=\"coupon_name\"\n      showArrow\n      v-model=\"list2Value\"\n      :showSearch=\"false\"\n      :list=\"list2\"\n      @change=\"onChange\"\n    ></st-select-scroll>\n    <st-hr></st-hr>\n  </div>\n</template>\n\n<script>\nexport default {\n  data() {\n    return {\n      model: 0,\n      keyword: '优惠券',\n      isLoading: false,\n      isEnd: false,\n      current_page: 1,\n      total: 10,\n      size: 10,\n      listValue: 1,\n      list: [\n        { name: '优惠券1', id: 1 },\n        { name: '优惠券2', id: 2 },\n        { name: '优惠券3', id: 3 },\n        { name: '优惠券4', id: 4 },\n        { name: '优惠券5', id: 5 },\n        { name: '优惠券6', id: 6 },\n        { name: '优惠券7', id: 7 },\n        { name: '优惠券8', id: 8 },\n        { name: '优惠券9', id: 9 },\n        { name: '优惠券10', id: 10 }\n      ],\n      list2Value: 1,\n      list2: [\n        { name: '优惠券1', id: 1 },\n        { name: '优惠券2', id: 2 },\n        { name: '优惠券3', id: 3 },\n        { name: '优惠券4', id: 4 },\n        { name: '优惠券5', id: 5 },\n        { name: '优惠券6', id: 6 },\n        { name: '优惠券7', id: 7 },\n        { name: '优惠券8', id: 8 },\n        { name: '优惠券9', id: 9 },\n        { name: '优惠券10', id: 10 }\n      ]\n    }\n  },\n  methods: {\n    // mock逻辑仅供参考\n    onSearch(isReload, query) {\n      if (query && query.coupon_name) this.keyword = query.coupon_name\n      if (isReload) {\n        this.current_page = 1\n        this.isEnd = false\n        this.total = Math.floor(Math.random() * 7) + 3\n      } else {\n        this.current_page += 1\n      }\n      this.isLoading = true\n      if (this.current_page > this.total) return\n      clearTimeout(this.timer)\n      this.timer = setTimeout(() => {\n        const list = []\n        for (let i = 0, len = this.total * this.current_page; i < len; i++) {\n          list.push({\n            name: this.keyword + (i + 1),\n            id: i + 1\n          })\n        }\n        this.list = list\n        this.isLoading = false\n        if (this.current_page >= this.total) this.isEnd = true\n      }, (Math.floor(Math.random() * 12) + 3) * 100)\n    },\n    onChange(e) {\n      console.log('change', e)\n    }\n  }\n}\n</script>\n"]}]}