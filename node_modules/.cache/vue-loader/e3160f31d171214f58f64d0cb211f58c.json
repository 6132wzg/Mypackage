ervice: H5SettingIndexService\n    }\n  },\n  watch: {},\n  data() {\n    return {\n      dropdownFlag: false\n    }\n  },\n  computed: {\n    selectShop: function() {\n      return this.shopList[this.shopActiveIndex]\n    }\n  },\n  methods: {\n    tapShop(shopId, index) {\n      this.dropdownFlag = !this.dropdownFlag\n      if (index !== this.shopActiveIndex) {\n        this.$emit('change', shopId)\n      }\n    }\n  }\n}\n</script>\n"]}]}