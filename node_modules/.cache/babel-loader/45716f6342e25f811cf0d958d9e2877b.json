 return {\n      config: AppConfig,\n      infoService: InfoService\n    };\n  },\n  rxState: function rxState() {\n    return {\n      personalCourseInfo: this.infoService.personalCourseInfo$\n    };\n  },\n  computed: {\n    coachColumns: coachColumns,\n    prices: function prices() {\n      var arr = [];\n      this.personalCourseInfo.price_gradient.forEach(function (item) {\n        item.prices.forEach(function (ele) {\n          arr.push(ele);\n        });\n      });\n      return arr;\n    },\n    image: function image() {\n      return this.personalCourseInfo.image.image_key;\n    },\n    saleModel: function saleModel() {\n      return this.personalCourseInfo.sale_model === 1; // 教练谈单\n    }\n  },\n  data: function data() {\n    return {\n      shopColumns: shopColumns,\n      priceConfigColumns: priceConfigColumns,\n      TRANSFER_UNIT: TRANSFER_UNIT\n    };\n  }\n};",{"version":3,"sources":["info.vue"],"names":[],"mappings":";;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;AA4JA,SACA,WADA,EAEA,YAFA,EAGA,kBAHA;AAKA,SAAA,WAAA;AACA,SAAA,SAAA,QAAA,oBAAA;AACA,SAAA,aAAA,QAAA,6BAAA;AACA,eAAA;AACA,EAAA,IAAA,EAAA,cADA;AAEA,EAAA,aAFA,2BAEA;AACA,WAAA;AACA,MAAA,MAAA,EAAA,SADA;AAEA,MAAA,WAAA,EAAA;AAFA,KAAA;AAIA,GAPA;AAQA,EAAA,OARA,qBAQA;AACA,WAAA;AACA,MAAA,kBAAA,EAAA,KAAA,WAAA,CAAA;AADA,KAAA;AAGA,GAZA;AAaA,EAAA,QAAA,EAAA;AACA,IAAA,YAAA,EAAA,YADA;AAEA,IAAA,MAFA,oBAEA;AACA,UAAA,GAAA,GAAA,EAAA;AACA,WAAA,kBAAA,CAAA,cAAA,CAAA,OAAA,CAAA,UAAA,IAAA,EAAA;AACA,QAAA,IAAA,CAAA,MAAA,CAAA,OAAA,CAAA,UAAA,GAAA,EAAA;AACA,UAAA,GAAA,CAAA,IAAA,CAAA,GAAA;AACA,SAFA;AAGA,OAJA;AAKA,aAAA,GAAA;AACA,KAVA;AAWA,IAAA,KAXA,mBAWA;AACA,aAAA,KAAA,kBAAA,CAAA,KAAA,CAAA,SAAA;AACA,KAbA;AAcA,IAAA,SAdA,uBAcA;AACA,aAAA,KAAA,kBAAA,CAAA,UAAA,KAAA,CAAA,CADA,CACA;AACA;AAhBA,GAbA;AA+BA,EAAA,IA/BA,kBA+BA;AACA,WAAA;AACA,MAAA,WAAA,EAAA,WADA;AAEA,MAAA,kBAAA,EAAA,kBAFA;AAGA,MAAA,aAAA,EAAA;AAHA,KAAA;AAKA;AArCA,CAAA","sourcesContent":["<template>\n  <st-panel-layout class=\"page-personal-course-info\">\n    <st-panel class=\" mg-b16\">\n      <div class=\"page-personal-header\">\n        <div class=\"page-personal-header__left mg-r24\">\n          <st-t3 class=\"mg-b16\">\n            {{ personalCourseInfo.course_name }}（{{\n              personalCourseInfo.category_id.name || '暂无'\n            }}）\n          </st-t3>\n          <div class=\"course-detail-item mg-b16\">\n            <div class=\"course-detail-item__left\">\n              <span class=\"label\">\n                <st-icon\n                  type=\"time\"\n                  color=\"rgb(0,199,219)\"\n                  class=\"mg-r8\"\n                  size=\"16px\"\n                ></st-icon>\n                时长:\n              </span>\n              <span class=\"value\">{{ personalCourseInfo.duration }}分钟</span>\n            </div>\n            <div class=\"course-detail-item__right\"></div>\n          </div>\n          <div class=\"course-detail-item__content mg-b16\">\n            {{ personalCourseInfo.description }}\n          </div>\n          <div class=\"course-detail-item__tip mg-b24\">\n            #\n            <span\n              v-for=\"(item, index) in personalCourseInfo.train_aim\"\n              :key=\"item.id\"\n            >\n              {{ item.name }}\n              <i v-if=\"index === personalCourseInfo.train_aim.length - 1\"></i>\n              /\n            </span>\n          </div>\n        </div>\n        <div class=\"page-personal-header__right\">\n          <img\n            :src=\"image | imgFilter({ w: 197, h: 136, type: 'course' })\"\n            alt=\"课程详情图片\"\n          />\n        </div>\n      </div>\n    </st-panel>\n    <st-panel app>\n      <div class=\"page-personal-content\">\n        <div class=\"page-personal-content__item mg-b24\">\n          <div class=\"title mg-b8\">\n            <span class=\"label\">上课门店:</span>\n            <span class=\"value\">\n              共{{ personalCourseInfo.shops.length }}家门店\n            </span>\n          </div>\n          <st-container>\n            <st-table\n              rowKey=\"shop_id\"\n              :columns=\"shopColumns\"\n              :dataSource=\"personalCourseInfo.shops\"\n              page-mode=\"client\"\n            ></st-table>\n          </st-container>\n        </div>\n        <div class=\"page-personal-content__item mg-b24\">\n          <div class=\"title mg-b8\">\n            <span class=\"label\">上课{{ $c('coach') }}:</span>\n            <span class=\"value\">\n              共{{ personalCourseInfo.coaches.length }}名{{ $c('coach') }}\n            </span>\n          </div>\n          <st-container>\n            <st-table\n              rowKey=\"id\"\n              :columns=\"coachColumns\"\n              page-mode=\"client\"\n              :dataSource=\"personalCourseInfo.coaches\"\n            >\n              <div slot=\"nature_work\" slot-scope=\"nature_work\">\n                {{ nature_work | enumFilter('staff.nature_work') }}\n              </div>\n            </st-table>\n          </st-container>\n        </div>\n        <div class=\"page-personal-content__item\">\n          <div class=\"title mg-b8\">\n            <span class=\"label\">售价设置:</span>\n            <span class=\"value\">\n              {{\n                personalCourseInfo.price_model\n                  | enumFilter('personal_course.price_model')\n              }}\n            </span>\n          </div>\n          <st-container>\n            <st-table\n              rowKey=\"id\"\n              :columns=\"priceConfigColumns\"\n              :dataSource=\"prices\"\n              page-mode=\"client\"\n            >\n              <div slot=\"min_sell_price\" slot-scope=\"min_sell_price, record\">\n                <span v-if=\"saleModel\">\n                  {{ record.min_sell_price }} ~ {{ record.max_sell_price }}\n                </span>\n                <span v-else>\n                  {{ record.sell_price }}\n                </span>\n              </div>\n              <div slot=\"min_sell\" slot-scope=\"min_sell, record\">\n                {{ record.min_sale }} ~ {{ record.max_sale }}\n              </div>\n              <div slot=\"transfer_num\" slot-scope=\"transfer_num, record\">\n                {{ record.transfer_num }}\n                {{\n                  record.transfer_unit === TRANSFER_UNIT.PERCENT ? '%' : '元'\n                }}\n              </div>\n            </st-table>\n          </st-container>\n          <div class=\"title mg-t16\">\n            <span class=\"label\">售卖方式:</span>\n            <span\n              class=\"value\"\n              v-for=\"(item, index) in personalCourseInfo.sell_type\"\n              :key=\"index\"\n            >\n              {{ item | enumFilter('personal_course.sell_type') }}\n              <i v-if=\"index !== personalCourseInfo.sell_type.length - 1\">、</i>\n            </span>\n          </div>\n          <div class=\"title mg-t8\">\n            <span class=\"label\">单节有效期:</span>\n            <span class=\"value\">{{ personalCourseInfo.effective_unit }}</span>\n          </div>\n          <div class=\"title mg-t8\">\n            <span class=\"label\">定价权限:</span>\n            <span class=\"value\">\n              {{\n                personalCourseInfo.price_setting\n                  | enumFilter('personal_course.price_setting')\n              }}\n            </span>\n          </div>\n          <div class=\"title mg-t8\">\n            <span class=\"label\">单节售卖:</span>\n            <span class=\"value\">{{ personalCourseInfo.single_price }}</span>\n          </div>\n        </div>\n      </div>\n    </st-panel>\n  </st-panel-layout>\n</template>\n<script>\nimport {\n  shopColumns,\n  coachColumns,\n  priceConfigColumns\n} from './info#table.config'\nimport { InfoService } from './info.service'\nimport { AppConfig } from '@/constants/config'\nimport { TRANSFER_UNIT } from '@/constants/course/personal'\nexport default {\n  name: 'PersonalInfo',\n  serviceInject() {\n    return {\n      config: AppConfig,\n      infoService: InfoService\n    }\n  },\n  rxState() {\n    return {\n      personalCourseInfo: this.infoService.personalCourseInfo$\n    }\n  },\n  computed: {\n    coachColumns,\n    prices() {\n      const arr = []\n      this.personalCourseInfo.price_gradient.forEach(item => {\n        item.prices.forEach(ele => {\n          arr.push(ele)\n        })\n      })\n      return arr\n    },\n    image() {\n      return this.personalCourseInfo.image.image_key\n    },\n    saleModel() {\n      return this.personalCourseInfo.sale_model === 1 // 教练谈单\n    }\n  },\n  data() {\n    return {\n      shopColumns,\n      priceConfigColumns,\n      TRANSFER_UNIT\n    }\n  }\n}\n</script>\n"],"sourceRoot":"src/views/pages/brand/product/course/personal"}]}