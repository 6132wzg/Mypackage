;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;AA6FA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA","file":"info.vue","sourceRoot":"src/views/pages/brand/product/course/team","sourcesContent":["<template>\n  <st-panel-layout class=\"page-team-info\">\n    <div class=\"header mg-b24\">\n      <div :class=\"b('left')\" class=\"mg-r24\">\n        <st-t3 class=\"mg-b16\">\n          {{ teamCourseInfo.course_name }}\n          <span v-if=\"teamCourseInfo.course_category.name\">\n            （{{ teamCourseInfo.course_category.name }}）\n          </span>\n        </st-t3>\n\n        <div :class=\"b('tip')\">\n          <div class=\"item\">\n            <st-icon\n              type=\"time\"\n              color=\"rgb(0,199,219)\"\n              class=\"mg-r8\"\n              size=\"16px\"\n            ></st-icon>\n            <span class=\"label mg-r8\">时长:</span>\n            <span class=\"value\">{{ teamCourseInfo.duration }}分钟</span>\n          </div>\n          <div class=\"item\">\n            <st-icon\n              type=\"money\"\n              size=\"16px\"\n              class=\"mg-r8\"\n              color=\"rgb(255,169,45)\"\n            ></st-icon>\n            <span class=\"label  mg-r8\">参考定价:</span>\n            <span class=\"value\">{{ teamCourseInfo.price }}元/节</span>\n          </div>\n          <div class=\"item\">\n            <st-icon\n              type=\"strength\"\n              size=\"16px\"\n              class=\"mg-r8\"\n              color=\"rgb(255,169,45)\"\n            ></st-icon>\n            <span class=\"label  mg-r8\">课程强度:</span>\n            <span class=\"value\">\n              <a-rate :defaultValue=\"teamCourseInfo.strength_level\" disabled />\n            </span>\n          </div>\n        </div>\n\n        <div\n          v-if=\"teamCourseInfo.description\"\n          :class=\"b('description')\"\n          class=\"mg-t16\"\n        >\n          {{ teamCourseInfo.description }}\n        </div>\n\n        <div :class=\"b('train-aim')\" class=\"mg-t16\">\n          #\n          {{ teamCourseInfo.train_aim | filterTrainAim }}\n        </div>\n      </div>\n\n      <div\n        class=\"page-team-header__right\"\n        v-viewer=\"{ url: 'data-src', toolbar: false }\"\n      >\n        <img\n          :src=\"image | imgFilter({ w: 560, h: 320, type: 'course' })\"\n          :data-src=\"image | imgFilter({ w: 1000, type: 'course' })\"\n          :alt=\"teamCourseInfo.course_name\"\n        />\n      </div>\n    </div>\n\n    <st-panel app :class=\"bb()\">\n      <div class=\"page-team-content__item mg-b24\">\n        <div class=\"title mg-b8\">\n          <span class=\"label\">上课门店:</span>\n          <span class=\"value\">\n            共{{ teamCourseInfo.support_shop_list.length }}家门店\n          </span>\n        </div>\n        <st-container>\n          <st-table\n            rowKey=\"shop_id\"\n            :dataSource=\"teamCourseInfo.support_shop_list\"\n            :columns=\"shopColumns\"\n            page-mode=\"client\"\n          ></st-table>\n        </st-container>\n      </div>\n    </st-panel>\n  </st-panel-layout>\n</template>\n<script>\nimport { shopColumns } from './info#table.config'\nimport { InfoService } from './info.service'\nexport default {\n  bem: {\n    b: 'header',\n    bb: 'body',\n    bAdv: 'page-dashboard-adv'\n  },\n  name: 'TeamCourseInfo',\n  serviceInject() {\n    return {\n      infoService: InfoService\n    }\n  },\n  rxState() {\n    return {\n      teamCourseInfo: this.infoService.teamCourseInfo$\n    }\n  },\n  computed: {\n    image() {\n      return this.teamCourseInfo.image.image_key\n    }\n  },\n  filters: {\n    filterTrainAim(val) {\n      return val\n        .map(item => {\n          return item.name\n        })\n        .join(' / ')\n    }\n  },\n  data() {\n    return {\n      shopColumns\n    }\n  }\n}\n</script>\n"]}]}