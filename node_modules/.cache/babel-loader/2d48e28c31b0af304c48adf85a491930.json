ndex\"\n            :value=\"item.value\"\n          >\n            {{ item.label }}\n          </a-select-option>\n        </a-select>\n        <a-select\n          :class=\"all('search__select')\"\n          class=\"mg-r8\"\n          v-model=\"$searchQuery.sell_status\"\n          @change=\"onSingleSearch('sell_status', $event)\"\n        >\n          <a-select-option\n            v-for=\"(item, index) in sellStatus\"\n            :key=\"index\"\n            :value=\"item.value\"\n          >\n            {{ item.label }}\n          </a-select-option>\n        </a-select>\n        <a-select\n          :class=\"all('search__select')\"\n          v-model=\"$searchQuery.card_number_type\"\n          :options=\"cardNumberTypeOptions\"\n          @change=\"onSingleSearch('support_member_num', $event)\"\n        ></a-select>\n      </div>\n    </div>\n    <st-table\n      :scroll=\"{ x: 1600 }\"\n      :page=\"page\"\n      @change=\"onTableChange\"\n      :loading=\"loading.getList\"\n      :columns=\"columns\"\n      :dataSource=\"list\"\n      rowKey=\"id\"\n    >\n      <!-- 卡名称 -->\n      <template slot=\"card_name\" slot-scope=\"text, record\">\n        <st-link\n          :value=\"text\"\n          :disabled=\"!record.auth['brand:product:member_card|get']\"\n          @click=\"goToCardInfo(record)\"\n        ></st-link>\n      </template>\n      <!-- 卡类型 -->\n      <template slot=\"card_type\" slot-scope=\"text\">{{ text.name }}</template>\n      <!-- 支持入场门店 -->\n      <template slot=\"admission_range\" slot-scope=\"text, record\">\n        <a\n          v-if=\"text.id === ADMISSION_RANGE.GENERAL_STORE\"\n          v-modal-link=\"{\n            name: 'card-brand-member-shop-table',\n            props: { id: record.id, type: 'Consume', title: '支持入场门店' }\n          }\"\n        >\n          {{ text.name }}\n        </a>\n        <span v-else class=\"use_num\">{{ text.name }}</span>\n      </template>\n      <!-- 支持售卖门店 -->\n      <template slot=\"support_sales\" slot-scope=\"text, record\">\n        <a\n          v-if=\"text.is_clickable\"\n          v-modal-link=\"{\n            name: 'card-brand-member-shop-table',\n            props: { id: record.id, type: 'Sale', title: '支持售卖门店' }\n          }\"\n        >\n          {{ text.name }}\n        </a>\n        <span v-else class=\"use_num\">{{ text.name }}</span>\n      </template>\n      <!-- 定价方式 -->\n      <template slot=\"price_setting\" slot-scope=\"text\">\n        {{ text.name }}\n      </template>\n      <!-- 有效期/有效次数 -->\n      <template slot=\"time_gradient\" slot-scope=\"text\">{{ text }}</template>\n      <!-- 售卖价格 -->\n      <template slot=\"price_gradient\" slot-scope=\"text, record\">\n        <card-specs\n          :value=\"record.price_gradients.name\"\n          :list=\"record.price_gradients.list\"\n        />\n      </template>\n      <!-- 上架/下架门店数 -->\n      <template slot=\"shelf\" slot-scope=\"text, record\">\n        <st-link\n          :value=\"`${record.shelf_upper}/${record.shelf_lower}`\"\n          :disabled=\"!record.shelf_upper && !record.shelf_lower\"\n          @click=\"showShelfShops(record)\"\n        ></st-link>\n      </template>\n      <!-- 售卖状态 -->\n      <template slot=\"sell_status\" slot-scope=\"text, record\">\n        <st-text\n          :status=\"{\n            success: text.id === 1,\n            error: text.id === 2\n          }\"\n        >\n          {{ text.name }}\n        </st-text>\n        <st-help-popover v-if=\"text.id === SELL_STATUS.NO_SELL\">\n          <span slot=\"title\">\n            操作人：{{ record.stop_sale.operator_name }}\n            <br />\n            操作时间：{{ record.stop_sale.stop_time }}\n          </span>\n          <span slot=\"content\">{{ record.stop_sale.stop_reason }}</span>\n        </st-help-popover>\n      </template>\n      <!-- 支持售卖时间 -->\n      <template slot=\"sell_time\" slot-scope=\"text, record\">\n        {{ record.start_time }}&nbsp;~&nbsp;{{ record.end_time }}\n      </template>\n      <!-- 发布渠道 -->\n      <template slot=\"publish_channel\" slot-scope=\"text\">\n        {{ text.name }}\n      </template>\n      <!-- 操作 -->\n      <div slot=\"action\" slot-scope=\"text, record\">\n        <st-table-actions>\n          <a\n            v-if=\"record.auth['brand:product:member_card|get']\"\n            @click=\"goToCardInfo(record)\"\n          >\n            详情\n          </a>\n          <a\n            v-if=\"record.auth['brand:product:member_card|del']\"\n            @click=\"onDelete(record)\"\n          >\n            删除\n          </a>\n          <a\n            v-if=\"record.auth['brand:product:member_card|down']\"\n            @click=\"onShelfDown(record)\"\n          >\n            下架\n          </a>\n          <a\n            v-if=\"record.auth['brand:product:member_card|edit']\"\n            @click=\"onEdit(record)\"\n          >\n            编辑\n          </a>\n          <a\n            v-if=\"record.auth['brand:product:member_card|up']\"\n            @click=\"onShelf(record)\"\n          >\n            上架\n          </a>\n          <a\n            v-if=\"record.auth['brand:product:member_card|pause']\"\n            @click=\"onStopSale(record)\"\n          >\n            停售\n          </a>\n          <a\n            v-if=\"record.auth['brand:product:member_card|restore']\"\n            @click=\"onRecoverSale(record)\"\n          >\n            恢复售卖\n          </a>\n          <a\n            v-if=\"record.auth['brand:product:member_card|sync_rights']\"\n            @click=\"\n              soldMemberCardActions.onUpdateRights(record.id, {\n                isSync: true,\n                isShowLink: false\n              })\n            \"\n          >\n            修改售出权益\n          </a>\n        </st-table-actions>\n      </div>\n    </st-table>\n  </div>\n</template>\n<script>\nimport { AllService } from './all.service'\nimport { columns, CARD_TYPE } from './all.config.ts'\nimport tableMixin from '@/mixins/table.mixin'\nimport CardBrandMemberRecoverSale from '@/views/biz-modals/card/brand-member/recover-sale'\nimport CardBrandMemberShelf from '@/views/biz-modals/card/brand-member/shelf'\nimport CardBrandMemberShopTable from '@/views/biz-modals/card/brand-member/shop-table'\nimport CardBrandMemberStopSale from '@/views/biz-modals/card/brand-member/stop-sale'\nimport CardShelfShops from '@/views/biz-modals/card/shelf-shops'\nimport CardSpecs from '@/views/biz-components/card/specs'\nimport soldMemberCardActions from '@/hooks/sold/member-card-actions.hook'\nimport {\n  ADMISSION_RANGE,\n  SUPPORT_SALES,\n  SELL_STATUS\n} from '@/constants/card/member'\nexport default {\n  mixins: [tableMixin],\n  name: 'PageBrandProductMemberAll',\n  bem: {\n    all: 'page-brand-product-member-list-all'\n  },\n  components: {\n    CardSpecs\n  },\n  modals: {\n    CardBrandMemberRecoverSale,\n    CardBrandMemberShelf,\n    CardBrandMemberShopTable,\n    CardBrandMemberStopSale,\n    CardShelfShops\n  },\n  serviceInject() {\n    return {\n      allService: AllService\n    }\n  },\n  rxState() {\n    return {\n      list: this.allService.list$,\n      page: this.allService.page$,\n      auth: this.allService.auth$,\n      loading: this.allService.loading$,\n      publishChannel: this.allService.publishChannel$,\n      cardType: this.allService.cardType$,\n      sellStatus: this.allService.sellStatus$,\n      cardNumberTypeOptions: this.allService.cardNumberTypeOptions$\n    }\n  },\n  data() {\n    return {\n      CARD_TYPE,\n      ADMISSION_RANGE,\n      SUPPORT_SALES,\n      SELL_STATUS\n    }\n  },\n  computed: {\n    columns\n  },\n  hooks() {\n    return {\n      soldMemberCardActions: soldMemberCardActions(this)\n    }\n  },\n  methods: {\n    /**\n     * 查看详情\n     */\n    goToCardInfo(record) {\n      this.$router.push({\n        path: `/brand/product/card/member/${\n          CARD_TYPE[record.card_type.id]\n        }/info`,\n        query: { id: record.id }\n      })\n    },\n    // 编辑\n    onEdit(record) {\n      const query = {\n        id: record.id\n      }\n      if (record.card_number_type.id === 2) {\n        query.type = 'family-card'\n      }\n      this.$router.push({\n        path: `/brand/product/card/member/${\n          this.CARD_TYPE[record.card_type.id]\n        }/edit`,\n        query\n      })\n    },\n    // 上架\n    onShelf(record) {\n      this.$modalRouter.push({\n        name: 'card-brand-member-shelf',\n        props: {\n          id: record.id,\n          title: `批量上架${this.$c('member_card')}`\n        },\n        on: {\n          success: () => {\n            this.$router.push({\n              query: { ...this.$searchQuery, current_page: 1 }\n            })\n          }\n        }\n      })\n    },\n    // 停售\n    onStopSale(record) {\n      this.$modalRouter.push({\n        name: 'card-brand-member-stop-sale',\n        props: {\n          id: +record.id,\n          cardType: record.card_type.id,\n          cardName: record.card_name\n        },\n        on: {\n          success: () => {\n            this.$router.push({\n              query: { ...this.$searchQuery, current_page: 1 }\n            })\n          }\n        }\n      })\n    },\n    // 恢复售卖\n    onRecoverSale(record) {\n      this.$modalRouter.push({\n        name: 'card-brand-member-recover-sale',\n        props: {\n          id: record.id,\n          cardType: record.card_type.id,\n          cardName: record.card_name\n        },\n        on: {\n          success: () => {\n            this.$router.push({\n              query: { ...this.$searchQuery, current_page: 1 }\n            })\n          }\n        }\n      })\n    },\n    // 删除\n    onDelete(record) {\n      this.$confirm({\n        title: '确认要删除',\n        content: `确认删除${record.card_name}${this.$c('member_card')}？`,\n        onOk: () => {\n          return this.allService\n            .deleteCard(record.id)\n            .toPromise()\n            .then(() => {\n              this.$router.push({\n                query: { ...this.$searchQuery, current_page: 1 }\n              })\n            })\n        }\n      })\n    },\n    showShelfShops(record) {\n      this.$modalRouter.push({\n        name: 'card-shelf-shops',\n        props: {\n          id: record.id,\n          title: `上架${record.shelf_upper}家/下架${record.shelf_lower}家`\n        }\n      })\n    }\n  }\n}\n</script>\n"],"sourceRoot":"src/views/pages/brand/product/card/member/list"}]}