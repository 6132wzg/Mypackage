ecorators,\n      show: false,\n      description: '',\n      batch_type: BATCH_TYPE.SELECTED\n    }\n  },\n  computed: {\n    helpText() {\n      return this.list_num > this.BATCH_INFO.LIMIT_NUM\n        ? this.BATCH_INFO.ERROR_TIP\n        : ''\n    },\n    helpShow() {\n      return this.list_num > this.BATCH_INFO.LIMIT_NUM\n    },\n    operateDataNum() {\n      return this.batch_type === this.BATCH_TYPE.SELECTED\n        ? this.ids.length\n        : this.list_num\n    }\n  },\n  methods: {\n    onSubmit() {\n      this.form.validate().then(values => {\n        values.extend_days = +values.extend_days\n        this.extengDayService\n          .taskExtendDays({\n            batch_type: this.batch_type,\n            sold_ids: this.ids,\n            conditions: this.searchQuery,\n            ...values\n          })\n          .subscribe(res => {\n            this.$emit('success')\n            this.show = false\n          })\n      })\n    }\n  }\n}\n",{"version":3,"sources":["extend-day.vue"],"names":[],"mappings":";;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;AAgEA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA","file":"extend-day.vue","sourceRoot":"src/views/biz-modals/sold/course","sourcesContent":["<template>\n  <st-modal\n    title=\"延长有效期\"\n    size=\"small\"\n    v-model=\"show\"\n    wrapClassName=\"modal-sold-card-giving1\"\n  >\n    <st-form :form=\"form\" labelWidth=\"75px\">\n      <st-form-item\n        label=\"选择\"\n        required\n        :help=\"helpText\"\n        :validateStatus=\"helpShow ? 'error' : ''\"\n      >\n        <st-batch-select-radio\n          :ids=\"this.ids\"\n          v-model=\"batch_type\"\n          :total=\"list_num\"\n        ></st-batch-select-radio>\n      </st-form-item>\n      <st-form-item label=\"延长天数\" required>\n        <st-input-number\n          :max=\"999\"\n          :min=\"1\"\n          placeholder=\"请输入延长天数\"\n          v-decorator=\"decorators.extend_days\"\n        >\n          <span slot=\"addonAfter\">\n            天\n          </span>\n        </st-input-number>\n      </st-form-item>\n      <st-form-item label=\"备注\" class=\"mg-b0\">\n        <a-textarea\n          v-decorator=\"decorators.content\"\n          placeholder=\"请输入备注\"\n          :autosize=\"{ minRows: 4, maxRows: 6 }\"\n        />\n      </st-form-item>\n    </st-form>\n    <template slot=\"footer\">\n      <st-button @click=\"show = false\">\n        取消\n      </st-button>\n      <st-popconfirm class=\"mg-l8\" @confirm=\"onSubmit\">\n        <div slot=\"title\" class=\"sold-popup-tip\">\n          <div class=\"sold-popup-tip__title\">\n            本次共选择{{ operateDataNum }}条记录，请再次确认是否提交！\n          </div>\n          <div class=\"sold-popup-tip__content\">\n            此操作将\n            <span class=\"color-danger\">覆盖</span>\n            当前选择项已有数据，将会有2-5s无法正常使用。\n          </div>\n        </div>\n        <st-button type=\"primary\" :loading=\"loading.setGive\">\n          确认提交\n        </st-button>\n      </st-popconfirm>\n    </template>\n  </st-modal>\n</template>\n\n<script>\nimport { ExtengDayService } from './extend-day.service'\nimport { ruleOptions } from './extend-day.config'\nimport { BATCH_TYPE, BATCH_INFO } from '@/constants/common/batch-operation'\nexport default {\n  name: 'ModalSoldCourseGiving',\n  serviceProviders() {\n    return [ExtengDayService]\n  },\n  serviceInject() {\n    return {\n      extengDayService: ExtengDayService\n    }\n  },\n  rxState() {\n    return {\n      list_num: this.extengDayService.list_num$,\n      loading: this.extengDayService.loading$\n    }\n  },\n  props: {\n    ids: {\n      type: Array,\n      required: true\n    },\n    searchQuery: {\n      type: Object\n    }\n  },\n  mounted() {\n    this.extengDayService.fetchCourseNum(this.searchQuery).subscribe()\n  },\n  data() {\n    const form = this.$stForm.create()\n    const decorators = form.decorators(ruleOptions)\n    return {\n      BATCH_TYPE,\n      BATCH_INFO,\n      form,\n      decorators,\n      show: false,\n      description: '',\n      batch_type: BATCH_TYPE.SELECTED\n    }\n  },\n  computed: {\n    helpText() {\n      return this.list_num > this.BATCH_INFO.LIMIT_NUM\n        ? this.BATCH_INFO.ERROR_TIP\n        : ''\n    },\n    helpShow() {\n      return this.list_num > this.BATCH_INFO.LIMIT_NUM\n    },\n    operateDataNum() {\n      return this.batch_type === this.BATCH_TYPE.SELECTED\n        ? this.ids.length\n        : this.list_num\n    }\n  },\n  methods: {\n    onSubmit() {\n      this.form.validate().then(values => {\n        values.extend_days = +values.extend_days\n        this.extengDayService\n          .taskExtendDays({\n            batch_type: this.batch_type,\n            sold_ids: this.ids,\n            conditions: this.searchQuery,\n            ...values\n          })\n          .subscribe(res => {\n            this.$emit('success')\n            this.show = false\n          })\n      })\n    }\n  }\n}\n</script>\n"]}]}