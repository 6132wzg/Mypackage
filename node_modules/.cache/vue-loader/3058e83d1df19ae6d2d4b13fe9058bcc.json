e\n    },\n    save() {\n      console.log(this.info)\n      this.reserveService.update({ ...this.info }).subscribe(() => {\n        this.messageService.success({\n          content: '提交成功'\n        })\n        this.show = false\n        this.$emit('change')\n      })\n    }\n  }\n}\n</script>\n"]}]}