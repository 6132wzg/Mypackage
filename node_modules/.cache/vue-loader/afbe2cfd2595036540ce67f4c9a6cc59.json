 <th>剩余课时</th>\n            <th></th>\n          </tr>\n        </thead>\n        <tbody>\n          <tr\n            class=\"table-item\"\n            v-for=\"(item, index) in packageCourseInfo\"\n            :key=\"index\"\n          >\n            <td></td>\n            <td>{{ item.course_name }}</td>\n            <td>{{ item.course_num_init }}</td>\n            <td>{{ item.course_num_remain }}</td>\n            <td></td>\n          </tr>\n        </tbody>\n      </st-form-table>\n      <st-hr class=\"mg-0\" />\n      <st-form :form=\"form\" labelWidth=\"88px\">\n        <div :class=\"transfer('transfer')\">\n          <member-search\n            :form=\"form\"\n            :saleRangeType=\"isCanAddNewMember\"\n          ></member-search>\n          <!-- <st-form-item class=\"mg-b0\" label=\"有效时间\" required >\n            <div :class=\"transfer('time')\">\n              <a-form-item class=\"page-a-form\">\n                <a-date-picker\n                  :disabledDate=\"disabledStartDate\"\n                  v-decorator=\"['startTime',{rules:[{validator:start_time_validator}]}]\"\n                  @change=\"onStartTimeChange\"\n                  :showTime=\"{format: 'HH:mm'}\"\n                  style=\"width: 100%;\"\n                  format=\"YYYY-MM-DD HH:mm\"\n                  placeholder=\"开始时间\"\n                  :allowClear=\"false\"\n                  :showToday=\"false\"\n                />\n              </a-form-item>\n              <span>~</span>\n              <a-form-item class=\"page-a-form\">\n                <a-date-picker\n                  disabled\n                  :value=\"endTime\"\n                  format=\"YYYY-MM-DD HH:mm\"\n                  placeholder=\"结束时间\"\n                  :showToday=\"false\"\n                />\n              </a-form-item>\n            </div>\n          </st-form-item>-->\n          <st-form-item required>\n            <template slot=\"label\">\n              剩余价值\n              <st-help-tooltip id=\"TS