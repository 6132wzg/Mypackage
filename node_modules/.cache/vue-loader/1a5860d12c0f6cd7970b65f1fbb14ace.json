\n      console.log('==========', res)\n    })\n  },\n  mounted() {\n    this.service.getShopList(this.id).subscribe(res => {\n      this.$nextTick().then(() => {\n        this.form.setFieldsValue({\n          shop_ids: this.shopOptions$.map(item => item.value)\n        })\n      })\n    })\n  }\n}\n",{"version":3,"sources":["offSales.vue"],"names":[],"mappings":";;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;AAyCA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA","file":"offSales.vue","sourceRoot":"src/views/biz-modals/package","sourcesContent":["<template>\n  <st-modal\n    title=\"下架课程包\"\n    @ok=\"onOffsale\"\n    okText=\"确认下架\"\n    width=\"500px\"\n    v-model=\"show\"\n  >\n    <div :class=\"bModal()\">\n      <st-form :form=\"form\">\n        <st-form-item label=\"课程包名称\" labelWidth=\"128px\">\n          {{ info$.course_name }}\n        </st-form-item>\n        <st-form-item label=\"当前上架门店(家)\" labelWidth=\"128px\">\n          {{ info$.upshelf_shop_num }}\n          <span :class=\"bModal('tip')\">（不含已下架门店）</span>\n        </st-form-item>\n        <st-form-item label=\"当前有效课程包\" labelWidth=\"128px\">\n          {{ info$.valid_package_num }}\n          <span :class=\"bModal('tip')\">（不含过期课程包）</span>\n        </st-form-item>\n        <st-form-item label=\"下架门店\" required labelWidth=\"128px\">\n          <a-select\n            v-decorator=\"decorators.shop_ids\"\n            mode=\"multiple\"\n            placeholder=\"请选择下架门店\"\n            :options=\"shopOptions$\"\n          ></a-select>\n        </st-form-item>\n        <st-form-item label=\"下架原因\" required labelWidth=\"128px\">\n          <st-textarea\n            v-decorator=\"decorators.reason\"\n            :maxlength=\"500\"\n            placeholder=\"请输入下架原因\"\n          ></st-textarea>\n        </st-form-item>\n      </st-form>\n    </div>\n  </st-modal>\n</template>\n<script>\nimport { OffSalesService } from './off-sales.service'\nimport { ruleOptions } from './off-sales.config'\nexport default {\n  name: 'PackageOffSales',\n  bem: {\n    bModal: 'modal-package-off-sales'\n  },\n  serviceInject() {\n    return {\n      service: OffSalesService\n    }\n  },\n  serviceProviders() {\n    return [OffSalesService]\n  },\n  rxState() {\n    const { info$, shopOptions$ } = this.service\n    return {\n      info$,\n      shopOptions$\n    }\n  },\n  props: {\n    id: [String, Number]\n  },\n  data() {\n    const form = this.$stForm.create()\n    const decorators = form.decorators(ruleOptions)\n    return {\n      form,\n      decorators,\n      show: false\n    }\n  },\n  methods: {\n    onOffsale() {\n      this.form.validate().then(values => {\n        this.service.setDownShelf({ id: this.id, ...values }).subscribe(res => {\n          this.$emit('success')\n          this.show = false\n        })\n      })\n    }\n  },\n  created() {\n    this.service.init({ id: this.id }).subscribe(res => {\n      console.log('==========', res)\n    })\n  },\n  mounted() {\n    this.service.getShopList(this.id).subscribe(res => {\n      this.$nextTick().then(() => {\n        this.form.setFieldsValue({\n          shop_ids: this.shopOptions$.map(item => item.value)\n        })\n      })\n    })\n  }\n}\n</script>\n"]}]}