default: function() {\n        return []\n      }\n    },\n    price: {\n      type: String,\n      default: ''\n    },\n    direction: {\n      type: String,\n      default: 'row'\n    },\n    tag: {\n      type: Number\n    }\n  },\n  computed: {\n    selfItemData: function() {\n      return this.itemData\n    }\n  },\n  components: {\n    H5Tag\n  },\n  data() {\n    return {\n      MINA_PRODUCT_TYPE,\n      banner2Options: {\n        freeMode: true,\n        slidesPerView: 'auto',\n        spaceBetween: 24\n      },\n      statusTextMap: {\n        1: '马上抢',\n        2: '未开始',\n        3: '已抢完'\n      }\n    }\n  },\n  methods: {\n    imgFilter\n  }\n}\n",{"version":3,"sources":["h5-phone.seckill.item.component.vue"],"names":[],"mappings":";;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;AA2GA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA","file":"h5-phone.seckill.item.component.vue","sourceRoot":"src/views/pages/brand/setting/mina/components#/h5/phone-components#","sourcesContent":["<template>\n  <div\n    :class=\"{\n      [b()]: true,\n      [b('row')]: direction === 'row',\n      [b('column')]: direction === 'column'\n    }\"\n  >\n    <template>\n      <div :class=\"b('header')\" v-if=\"direction === 'column'\">\n        <span>\n          {{ item.activity_status !== 1 ? '距秒杀开始' : '距秒杀结束' }}\n        </span>\n        <span class=\"clock-timer\">\n          <st-clock\n            :value=\"\n              item.activity_status !== 1\n                ? item.start_time * 1000\n                : item.end_time * 1000\n            \"\n            format=\"[DD天]hh:mm:ss\"\n          ></st-clock>\n        </span>\n      </div>\n      <div :class=\"b('content')\">\n        <div :class=\"b('image')\">\n          <img\n            :class=\"b('image-bg')\"\n            :width=\"direction === 'column' ? 200 : 300\"\n            :height=\"direction === 'column' ? 150 : 225\"\n            :src=\"\n              imgFilter(\n                image,\n                direction === 'column' ? { w: 200, h: 150 } : { w: 300, h: 225 }\n              )\n            \"\n            alt=\"\"\n          />\n          <h5-tag :class=\"b('image-tag')\" :tag=\"tag\"></h5-tag>\n        </div>\n        <div :class=\"b('info')\">\n          <div :class=\"b('info-name')\">{{ name }}</div>\n          <div\n            :class=\"b('info-desc')\"\n            v-if=\"item.product_type !== MINA_PRODUCT_TYPE.PERSONAL_COURSE\"\n          >\n            {{ item.spec_desc }}\n          </div>\n          <div\n            :class=\"b('info-tags')\"\n            v-if=\"item.product_type === MINA_PRODUCT_TYPE.PERSONAL_COURSE\"\n          >\n            <span\n              class=\"text-line1\"\n              :class=\"b('info-tag')\"\n              v-for=\"(tag, index) in tags\"\n              :key=\"index\"\n            >\n              {{ tag.name || tag }}\n            </span>\n          </div>\n          <div :class=\"b('info-bottom')\">\n            <div :class=\"b('info-bottom-side')\">\n              <span :class=\"b('info-bottom-price')\">{{ item.price }}</span>\n              <!-- MINA_PRODUCT_TYPE.PERSONAL_COURSE 私教课类型 -->\n              <span\n                :class=\"b('info-bottom-line-price')\"\n                v-if=\"item.product_type !== MINA_PRODUCT_TYPE.PERSONAL_COURSE\"\n              >\n                ¥{{ item.original_price }}\n              </span>\n              <span\n                :class=\"b('info-bottom-tag')\"\n                v-if=\"item.product_type === MINA_PRODUCT_TYPE.PERSONAL_COURSE\"\n              >\n                限定{{ item.init_course_num }}节\n              </span>\n            </div>\n            <div :class=\"b('info-bottom-side')\">\n              <span\n                :class=\"{\n                  [b('info-bottom-mark')]: true,\n                  [b('info-bottom-mark--nostart')]: item.activity_status === 2,\n                  [b('info-bottom-mark--runing')]: item.activity_status === 1,\n                  [b('info-bottom-mark--complete')]: item.activity_status === 3\n                }\"\n              >\n                <span class=\"clock-timer\" v-if=\"direction === 'row'\">\n                  <st-clock\n                    :value=\"\n                      item.activity_status !== 1\n                        ? item.start_time * 1000\n                        : item.end_time * 1000\n                    \"\n                    format=\"[DD天]hh:mm:ss\"\n                  ></st-clock>\n                </span>\n                <span>{{ statusTextMap[item.activity_status] }}</span>\n              </span>\n            </div>\n          </div>\n        </div>\n      </div>\n    </template>\n  </div>\n</template>\n<script>\nimport { MINA_PRODUCT_TYPE } from '@/constants/brand/app/plugin/discount'\nimport { imgFilter } from '@/filters/resource.filters'\nimport H5Tag from '@/views/biz-components/h5/h5-tag/h5-tag'\nexport default {\n  name: 'h5PhoneRecommendItemComponent',\n  bem: {\n    b: 'h5-phone-seckill-item-component'\n  },\n  props: {\n    item: {\n      type: Object,\n      default: () => ({\n        tags: []\n      })\n    },\n    image: {\n      type: String,\n      default: ''\n    },\n    name: {\n      type: String,\n      default: ''\n    },\n    desc: {\n      type: String,\n      default: ''\n    },\n    tags: {\n      type: Array,\n      default: function() {\n        return []\n      }\n    },\n    price: {\n      type: String,\n      default: ''\n    },\n    direction: {\n      type: String,\n      default: 'row'\n    },\n    tag: {\n      type: Number\n    }\n  },\n  computed: {\n    selfItemData: function() {\n      return this.itemData\n    }\n  },\n  components: {\n    H5Tag\n  },\n  data() {\n    return {\n      MINA_PRODUCT_TYPE,\n      banner2Options: {\n        freeMode: true,\n        slidesPerView: 'auto',\n        spaceBetween: 24\n      },\n      statusTextMap: {\n        1: '马上抢',\n        2: '未开始',\n        3: '已抢完'\n      }\n    }\n  },\n  methods: {\n    imgFilter\n  }\n}\n</script>\n"]}]}