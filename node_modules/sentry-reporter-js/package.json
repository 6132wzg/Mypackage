{
  "name": "sentry-reporter-js",
  "version": "0.3.6",
  "scripts": {
    "dev": "rollup -c -w -f umd -o example/static/sentry-reporter.js",
    "build:umd": "rollup -c -f umd -o dist/sentry-reporter.js",
    "build:umd.min": "rollup -c rollup.prod.js -f umd -o dist/sentry-reporter.min.js",
    "build:cjs": "rollup -c -f cjs -o dist/sentry-reporter.common.js",
    "build": "npm run build:cjs && npm run build:umd.min && npm run build:umd"
  },
  "main": "./dist/sentry-reporter.common.js",
  "devDependencies": {
    "babel": "^6.23.0",
    "babel-core": "^6.26.3",
    "babel-plugin-external-helpers": "^6.22.0",
    "babel-preset-env": "^1.7.0",
    "morgan": "^1.9.0",
    "rollup": "^0.64.1",
    "rollup-plugin-babel": "^3.0.7",
    "rollup-plugin-commonjs": "^9.1.5",
    "rollup-plugin-node-resolve": "^3.3.0",
    "rollup-plugin-replace": "^2.0.0",
    "rollup-plugin-uglify": "^4.0.0"
  },
  "dependencies": {
    "path-to-regexp": "^2.2.1",
    "qs": "^6.5.2",
    "raven-js": "^3.26.4"
  }
}
