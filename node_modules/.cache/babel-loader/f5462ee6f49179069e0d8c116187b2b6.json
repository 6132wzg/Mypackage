  };\n  },\n  rxState: function rxState() {\n    return {\n      loading: this.service.loading$,\n      list: this.service.list$,\n      page: this.service.page$,\n      cocach_type: this.service.cocach_type$\n    };\n  },\n  data: function data() {\n    return {\n      show: false\n    };\n  },\n  computed: {\n    columns: columns\n  },\n  mounted: function mounted() {\n    this.init();\n  },\n  methods: {\n    getList: function getList() {\n      console.log('我是新增的限时折扣');\n      this.service.getGroupBuyList(this.query).subscribe();\n    },\n    onSubmit: function onSubmit() {\n      var data = {\n        selected: this.selectedRowKeys,\n        selectedRows: this.selectedRows || []\n      };\n      if (this.handlerValidate && !this.handlerValidate(data)) return;\n      this.show = false;\n      console.log('complete', data);\n      this.handlerSubmit && this.handlerSubmit(data);\n      this.$emit('complete', data);\n    },\n    onCancel: function onCancel() {\n      this.show = false;\n    },\n    handlerValidate: function handlerValidate() {\n      if (this.selectedRowKeys.length > this.max) {\n        this.messageService.error({\n          content: '当前数量已达上限，请在删除后重新添加'\n        });\n        return false;\n      }\n\n      return true;\n    }\n  }\n};",{"version":3,"sources":["index.vue"],"names":[],"mappings":";;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;AAgEA,SAAA,eAAA,IAAA,OAAA;AACA,SAAA,yBAAA;AACA,SAAA,cAAA,QAAA,4BAAA;AACA,OAAA,UAAA,MAAA,uBAAA;AAEA,eAAA;AACA,EAAA,MAAA,EAAA,CAAA,UAAA,CADA;AAEA,EAAA,GAAA,EAAA;AACA,IAAA,CAAA,EAAA;AADA,GAFA;AAKA,EAAA,KAAA,EAAA;AACA,IAAA,KAAA,EAAA;AACA,MAAA,IAAA,EAAA;AADA,KADA;;AAIA;;;AAGA,IAAA,GAAA,EAAA;AACA,MAAA,IAAA,EAAA,MADA;AAEA,MAAA,OAAA,EAAA;AAFA;AAPA,GALA;AAkBA,EAAA,aAlBA,2BAkBA;AACA,WAAA;AACA,MAAA,cAAA,EAAA,cADA;AAEA,MAAA,OAAA,EAAA;AAFA,KAAA;AAIA,GAvBA;AAwBA,EAAA,OAxBA,qBAwBA;AACA,WAAA;AACA,MAAA,OAAA,EAAA,KAAA,OAAA,CAAA,QADA;AAEA,MAAA,IAAA,EAAA,KAAA,OAAA,CAAA,KAFA;AAGA,MAAA,IAAA,EAAA,KAAA,OAAA,CAAA,KAHA