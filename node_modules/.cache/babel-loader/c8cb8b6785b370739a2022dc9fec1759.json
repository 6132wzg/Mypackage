r=\"(item, index) in teamTemplateList\">\n            <tr :key=\"index\">\n              <td>{{ item.created_time }}</td>\n              <td>{{ item.template_name }}</td>\n              <td>{{ item.course_type_name }}</td>\n              <td>{{ item.max_number ? item.max_number : '--' }}</td>\n              <td v-if=\"type !== 'small'\">{{ item.reserve_setting_text }}</td>\n              <td>{{ item.operator_name }}</td>\n              <td>\n                <template>\n                  <a class=\"mg-r16\" @click=\"editRow(item.id)\">\n                    编辑\n                  </a>\n                  <!-- 仅删除本地数据 -->\n                  <a @click=\"delRow(item.id, index)\">\n                    删除\n                  </a>\n                </template>\n              </td>\n            </tr>\n          </template>\n        </tbody>\n      </st-form-table>\n    </div>\n  </st-modal>\n</template>\n<script>\nexport default {\n  name: 'courseManageService',\n  props: {\n    teamTemplateList: {\n      type: Array,\n      default: () => {\n        return []\n      }\n    },\n    type: {\n      type: String,\n      default: ''\n    }\n  },\n  data() {\n    return {\n      show: false,\n      idArr: []\n    }\n  },\n  methods: {\n    // 新增课表\n    addCourse() {\n      this.$emit('add')\n      this.show = false\n    },\n    // 编辑课表\n    editRow(id) {\n      this.$emit('edit', id)\n      this.show = false\n    },\n    // 点击删除，暂存删除id\n    delRow(id, index) {\n      this.teamTemplateList.splice(index, 1)\n      this.idArr.push(id)\n    },\n    // 确定删除\n    save() {\n      this.$emit('save', this.idArr)\n      this.show = false\n    }\n  }\n}\n</script>\n"],"sourceRoot":"src/views/biz-modals/schedule/batch"}]}