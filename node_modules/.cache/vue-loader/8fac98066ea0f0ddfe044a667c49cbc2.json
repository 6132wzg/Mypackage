   return {\n      show: false\n    }\n  },\n  computed: {\n    columns\n  },\n  mounted() {\n    this.init()\n  },\n  methods: {\n    moment,\n    getList() {\n      console.log('我是新增的限时打折')\n      this.service.getDiscountList(this.searchQuery).subscribe()\n    },\n    onSubmit() {\n      const data = {\n        selected: this.selectedRowKeys,\n        selectedRows: this.selectedRows || []\n      }\n      if (this.handlerValidate && !this.handlerValidate(data)) return\n      this.show = false\n      console.log('complete', data)\n      this.handlerSubmit && this.handlerSubmit(data)\n      this.$emit('complete', data)\n    },\n    onCancel() {\n      this.show = false\n    },\n    handlerValidate() {\n      if (this.selectedRowKeys.length > this.max) {\n        this.messageService.error({\n          content: '当前数量已达上限，请在删除后重新添加'\n        })\n        return false\n      }\n      return true\n    }\n  }\n}\n",{"version":3,"sources":["discount-table-select.vue"],"names":[],"mappings":";;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;AAsEA;AACA;AACA;AACA;;AAEA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;;AAEA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;;AAEA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA","file":"discount-table-select.vue","sourceRoot":"src/views/biz-modals/custom-decorate","sourcesContent":["<template>\n  <st-modal :title=\"title\" v-model=\"show\" @ok=\"onSubmit\" @cancel=\"onCancel\">\n    <div>\n      <st-header-actions type=\"small\">\n        <template slot=\"left\">\n          <div class=\"color-danger\" v-if=\"selectedRowKeys.length > max\">\n            最多选择{{ max }}项\n          </div>\n          <div v-else>\n            已选{{ selectedRowKeys.length }}项\n          </div>\n        </template>\n        <template slot=\"right\">\n          <st-select\n            class=\"mg-r12\"\n            v-model=\"searchQuery.activity_status\"\n            :options=\"activityStatus\"\n            @change=\"onSingleSearch('activity_status', $event)\"\n          ></st-select>\n          <st-input-search\n            style=\"width: 240px\"\n            @change=\"onSingleSearch('activity_name', $event.target.value)\"\n            placeholder=\"搜索\"\n          ></st-input-search>\n        </template>\n      </st-header-actions>\n      <st-container>\n        <st-table\n          rowKey=\"activity_id\"\n          :loading=\"loading.getDiscountList\"\n          :columns=\"columns\"\n          :dataSource=\"list\"\n          :page=\"page\"\n          :rowSelection=\"{\n            selectedRowKeys: selectedRowKeys,\n            onChange: onSelectionChange,\n            getCheckboxProps: record => ({\n              props: {\n                disabled: record.activity_status === 3\n              }\n            })\n          }\"\n          :scroll=\"{ x: 600 }\"\n          :simplePage=\"true\"\n          @change=\"onTableChange\"\n        >\n          <span slot=\"product_type\" slot-scope=\"text\">\n            {{ text | enumFilter('mina_setting.mina_product_type') }}\n          </span>\n          <span slot=\"temp\" slot-scope=\"text, record\">\n            {{\n              moment(record.active_start_time).format('YYYY-MM-DD hh:mm:ss') ||\n                '-- ' +\n                  '~' +\n                  moment(record.active_end_time).format(\n                    'YYYY-MM-DD hh:mm:ss'\n                  ) ||\n                ' --'\n            }}\n          </span>\n          <span slot=\"activity_status\" slot-scope=\"text\">\n            {{ text | enumFilter('limited_time_discount.activity_status') }}\n          </span>\n        </st-table>\n      </st-container>\n    </div>\n  </st-modal>\n</template>\n\n<script>\nimport { Discountcolumns as columns } from './discount-table-select.config'\nimport { DiscountTableSelectService } from './discount-table-select.service'\nimport { MessageService } from '@/services/message.service'\nimport tableMixin from '@/mixins/table2.mixin'\n\nexport default {\n  mixins: [tableMixin],\n  bem: {\n    b: 'custom-decorate-product-table-select'\n  },\n  props: {\n    title: {\n      type: String\n    },\n    /**\n     * 最大选择数量\n     */\n    max: {\n      type: Number,\n      default: 10\n    }\n  },\n\n  serviceInject() {\n    return {\n      messageService: MessageService,\n      service: DiscountTableSelectService\n    }\n  },\n  rxState() {\n    return {\n      loading: this.service.loading$,\n      list: this.service.list$,\n      page: this.service.page$,\n      activityStatus: this.service.activityStatus$\n    }\n  },\n\n  data() {\n    return {\n      show: false\n    }\n  },\n  computed: {\n    columns\n  },\n  mounted() {\n    this.init()\n  },\n  methods: {\n    moment,\n    getList() {\n      console.log('我是新增的限时打折')\n      this.service.getDiscountList(this.searchQuery).subscribe()\n    },\n    onSubmit() {\n      const data = {\n        selected: this.selectedRowKeys,\n        selectedRows: this.selectedRows || []\n      }\n      if (this.handlerValidate && !this.handlerValidate(data)) return\n      this.show = false\n      console.log('complete', data)\n      this.handlerSubmit && this.handlerSubmit(data)\n      this.$emit('complete', data)\n    },\n    onCancel() {\n      this.show = false\n    },\n    handlerValidate() {\n      if (this.selectedRowKeys.length > this.max) {\n        this.messageService.error({\n          content: '当前数量已达上限，请在删除后重新添加'\n        })\n        return false\n      }\n      return true\n    }\n  }\n}\n</script>\n"]}]}