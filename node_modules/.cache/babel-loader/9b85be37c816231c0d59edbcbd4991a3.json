\n    return {\n      info: this.infoService.info$\n    };\n  },\n  computed: {\n    price_gradient: function price_gradient() {\n      var arr = [];\n      this.info.price_gradient.forEach(function (item) {\n        item.prices.forEach(function (ele) {\n          arr.push(ele);\n        });\n      });\n      return arr;\n    },\n    image: function image() {\n      return this.info.image.image_key;\n    },\n    coachColumns: coachColumns\n  },\n  data: function data() {\n    return {\n      shopColumns: shopColumns,\n      priceConfigColumns: priceConfigColumns\n    };\n  }\n};",{"version":3,"sources":["info.vue"],"names":[],"mappings":";;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;AA6HA,SACA,WADA,EAEA,YAFA,EAGA,kBAHA;AAKA,SAAA,WAAA;AACA,SAAA,SAAA,QAAA,4BAAA;AACA,SAAA,SAAA,QAAA,oBAAA;AACA,eAAA;AACA,EAAA,IAAA,EAAA,cADA;AAEA,EAAA,aAFA,2BAEA;AACA,WAAA;AACA,MAAA,WAAA,EAAA,WADA;AAEA,MAAA,SAAA,EAAA;AAFA,KAAA;AAIA,GAPA;AAQA,EAAA,OARA,qBAQA;AACA,WAAA;AACA,MAAA,IAAA,EAAA,KAAA,WAAA,CAAA;AADA,KAAA;AAGA,GAZA;AAaA,EAAA,QAAA,EAAA;AACA,IAAA,cADA,4BACA;AACA,UAAA,GAAA,GAAA,EAAA;AACA,WAAA,IAAA,CAAA,cAAA,CAAA,OAAA,CAAA,UAAA,IAAA,EAAA;AACA,QAAA,IAAA,CAAA,MAAA,CAAA,OAAA,CAAA,UAAA,GAAA,EAAA;AACA,UAAA,GAAA,CAAA,IAAA,CAAA,GAAA;AACA,SAFA;AAGA,OAJA;AAKA,aAAA,GAAA;AACA,KATA;AAUA,IAAA,KAVA,mBAUA;AACA,aAAA,KAAA,IAAA,CAAA,KAAA,CAAA,SAAA;AACA,KAZA;AAaA,IAAA,YAAA,EAAA;AAbA,GAbA;AA4BA,EAAA,IA5BA,kBA4BA;AACA,WAAA;AACA,MAAA,WAAA,EAAA,WADA;AAEA,MAAA,kBAAA,EAAA;AAFA,KAAA;AAIA;AAjCA,CAAA","sourcesContent":["<template>\n  <st-panel-layout class=\"page-personal-course-info\">\n    <st-panel class=\"mg-b16\">\n      <div class=\"page-personal-header\">\n        <div class=\"page-personal-header__left mg-r24\">\n          <st-t3 class=\"mg-b16\">\n            {{ info.course_name }}（{{ info.category_id.name || '暂无' }}）\n          </st-t3>\n          <div class=\"course-detail-item mg-b16\">\n            <div class=\"course-detail-item__left\">\n              <span class=\"label\">\n                <st-icon\n                  type=\"time\"\n                  color=\"rgb(0,199,219)\"\n                  class=\"mg-r8\"\n                  size=\"16px\"\n                ></st-icon>\n                时长:\n              </span>\n              <span class=\"value\">{{ info.duration }}分钟</span>\n            </div>\n          </div>\n          <div class=\"course-detail-item__content mg-b16\">\n            {{ info.description }}\n          </div>\n          <div class=\"course-detail-item__tip mg-b24\">\n            #\n            <span v-for=\"(item, index) in info.train_aim\" :key=\"item.id\">\n              {{ item.name }}\n              <i v-if=\"index === info.train_aim.length - 1\"></i>\n              /\n            </span>\n          </div>\n        </div>\n        <div\n          class=\"page-personal-header__right\"\n          v-viewer=\"{ url: 'data-src', toolbar: false }\"\n        >\n          <img\n            :src=\"image | imgFilter({ w: 560, h: 320, type: 'course' })\"\n            :data-src=\"image | imgFilter({ w: 1000, type: 'course' })\"\n            :alt=\"info.course_name\"\n          />\n        </div>\n      </div>\n    </st-panel>\n    <st-panel app>\n      <div class=\"page-personal-content\">\n        <div class=\"page-personal-content__item mg-b24\">\n          <div class=\"title mg-b8\">\n            <span class=\"label\">上课门店:</span>\n            <span class=\"value\">共{{ info.shops.length }}家门店</span>\n          </div>\n          <st-container>\n            <st-table\n              :columns=\"shopColumns\"\n              rowKey=\"shop_id\"\n              :dataSource=\"info.shops\"\n              page-mode=\"client\"\n            ></st-table>\n          </st-container>\n        </div>\n        <div class=\"page-personal-content__item mg-b24\">\n          <div class=\"title mg-b8\">\n            <span class=\"label\">上课{{ $c('coach') }}:</span>\n            <span class=\"value\">\n              共{{ info.coaches.length }}名{{ $c('coach') }}\n            </span>\n          </div>\n          <st-container>\n            <st-table\n              :columns=\"coachColumns\"\n              rowKey=\"id\"\n              page-mode=\"client\"\n              :dataSource=\"info.coaches\"\n            >\n              <div slot=\"nature_work\" slot-scope=\"nature_work\">\n                {{ nature_work | enumFilter('staff.nature_work') }}\n              </div>\n            </st-table>\n          </st-container>\n        </div>\n        <div class=\"page-personal-content__item\">\n          <!-- <div class=\"title mg-b8\"><span class=\"label\">售价渠道:</span><span class=\"value\">{{price_gradient}}</span></div>\n        <div class=\"title mg-b8\"><span class=\"label\">单节有效期:</span><span class=\"value\">{{price_gradient}}</span></div>\n        <div class=\"title mg-b8\"><span class=\"label\">定价权限:</span><span class=\"value\">{{price_gradient}}</span></div>\n        <div class=\"title mg-b8\"><span class=\"label\">单节售卖:</span><span class=\"value\">{{price_gradient.single_price}}</span></div>-->\n          <div class=\"title mg-b8\">\n            <span class=\"label\">售卖定价:</span>\n            <span class=\"value\">\n              {{\n                info.price_setting | enumFilter('personal_course.price_setting')\n              }}\n            </span>\n          </div>\n          <st-container>\n            <st-table\n              :columns=\"priceConfigColumns\"\n              rowKey=\"id\"\n              :dataSource=\"price_gradient\"\n              page-mode=\"client\"\n            >\n              <div slot=\"min_sell_price\" slot-scope=\"min_sell_price, record\">\n                <span v-if=\"record.min_sell_price !== '0.0'\">\n                  {{ record.min_sell_price }} ~ {{ record.max_sell_price }}\n                </span>\n                <span v-else>\n                  {{ record.sell_price }}\n                </span>\n              </div>\n              <div slot=\"min_sell\" slot-scope=\"min_sell, record\">\n                {{ record.min_sale }} ~ {{ record.max_sale }}\n              </div>\n              <div slot=\"transfer_num\" slot-scope=\"transfer_num, record\">\n                {{ record.transfer_num }}\n                {{ record.transfer_unit === 1 ? '%' : '元' }}\n              </div>\n            </st-table>\n          </st-container>\n        </div>\n      </div>\n    </st-panel>\n  </st-panel-layout>\n</template>\n<script>\nimport {\n  shopColumns,\n  coachColumns,\n  priceConfigColumns\n} from './info#table.config'\nimport { InfoService } from './info.service'\nimport { imgFilter } from '@/filters/resource.filters'\nimport { AppConfig } from '@/constants/config'\nexport default {\n  name: 'PersonalInfo',\n  serviceInject() {\n    return {\n      infoService: InfoService,\n      appConfig: AppConfig\n    }\n  },\n  rxState() {\n    return {\n      info: this.infoService.info$\n    }\n  },\n  computed: {\n    price_gradient() {\n      const arr = []\n      this.info.price_gradient.forEach(item => {\n        item.prices.forEach(ele => {\n          arr.push(ele)\n        })\n      })\n      return arr\n    },\n    image() {\n      return this.info.image.image_key\n    },\n    coachColumns\n  },\n  data() {\n    return {\n      shopColumns,\n      priceConfigColumns\n    }\n  }\n}\n</script>\n"],"sourceRoot":"src/views/pages/shop/product/course/manage/personal"}]}